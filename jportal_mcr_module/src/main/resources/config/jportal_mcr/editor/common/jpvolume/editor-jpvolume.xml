    <panel id="edit-jpvolume" lines="on">
        <hidden var="ID" />
        <hidden var="label" />
        <hidden var="metadata/@lang" default="de" />
        <hidden var="service/servdates" descendants="true" />
        <hidden var="service/servacls" descendants="true" />
        <hidden var="service/servflags" descendants="true" />
        <hidden var="metadata/derivateLinks" descendants="true" />

        <!-- ########################## formal description ################################## -->
        <cell anchor="WEST" colspan="4" col="1" row="1" class="editorHeadlines">
            <text>
                <label i18n="metaData.headlines.formaldiscr" />
            </text>
        </cell>
            <!-- maintitle -->
        <cell class="editorMandatory" row="10" col="1" anchor="NORTHWEST" ref="maintitle_label" />
        <cell row="10" col="2" anchor="NORTHWEST" ref="maintitle_input" />
            <!-- subtitle -->
        <cell class="editorLabel" row="20" col="1" anchor="NORTHWEST" ref="subtitle_label" />
        <cell row="20" col="2" anchor="NORTHWEST" ref="subtitle_input" />
            <!-- partic -->
        <cell class="editorLabel" row="30" col="1" anchor="NORTHWEST" ref="participants_label" />
        <cell row="30" col="2" anchor="NORTHWEST" ref="participants_input" />
            <!-- date -->
        <cell class="editorLabel" row="40" col="1" anchor="NORTHWEST" ref="date_label" />
        <!-- tradition -->
        <cell class="editorLabel" row="41" col="1" anchor="NORTHWEST" ref="tradition_label" />
        <cell row="41" col="2" anchor="NORTHWEST" ref="tradition_input" />
        <!-- collation note -->
        <cell class="editorLabel" row="50" col="1" anchor="NORTHWEST" ref="collation_note_label" />
        <cell row="50" col="2" anchor="NORTHWEST" ref="collation_note_input" />
        <cell row="40" col="2" anchor="NORTHWEST" ref="date_input" />
        <!-- identi -->
        <cell class="editorLabel" row="45" col="1" anchor="NORTHWEST" ref="identi_label" />
        <cell row="45" col="2" anchor="NORTHWEST" ref="identi_input" />
        <cell col="1" row="60">
            <br />
        </cell>
        <!-- ########################## content description ################################## -->
        <cell anchor="WEST" colspan="4" col="1" row="61" class="editorHeadlines">
            <text>
                <label css-style="editorHeadlines" i18n="metaData.headlines.contantdiscr" />
            </text>
        </cell>
            <!-- abstract -->
        <cell class="editorLabel" row="80" col="1" anchor="NORTHWEST" ref="abstract_label" />
        <cell row="80" col="2" anchor="NORTHWEST" ref="abstract_input" />
        
            <!-- note -->
        <cell class="editorLabel" row="160" col="1" anchor="NORTHWEST" ref="note_label" />
        <cell row="160" col="2" anchor="NORTHWEST" ref="note_input" />
        
            <!-- person -->
        <cell class="editorLabel" row="162" col="1" anchor="NORTHWEST" ref="person_label" />
        <cell row="162" col="2" anchor="NORTHWEST" ref="person_input" />
        
            <!-- publication note -->
        <cell class="editorLabel" row="164" col="1" anchor="NORTHWEST" ref="publicationNote_label" />
        <cell row="164" col="2" anchor="NORTHWEST" ref="publicationNote_input" />
            
            <!-- normed pubLocation -->
        <cell class="editorLabel" row="166" col="1" anchor="NORTHWEST" ref="normedPubLocation_label" />
        <cell row="166" col="2" anchor="NORTHWEST" ref="normedPubLocation_input" />
        
            <!-- foot note -->
        <cell class="editorLabel" row="168" col="1" anchor="NORTHWEST" ref="footNote_label" />
        <cell row="168" col="2" anchor="NORTHWEST" ref="footNote_input" />
        
            <!-- bib evidence -->
        <cell class="editorLabel" row="170" col="1" anchor="NORTHWEST" ref="bibEvidence_label" />
        <cell row="170" col="2" anchor="NORTHWEST" ref="bibEvidence_input" />

            <!-- index field -->
        <cell class="editorLabel" row="172" col="1" anchor="NORTHWEST" ref="indexField_label" />
        <cell row="172" col="2" anchor="NORTHWEST" ref="indexField_input" />

            <!-- ================================================================================================= -->
            <!-- dynamic includes, if used within a journal -->
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_classiVol1/hidden_classiVol1:editor/editor-dynamicInludes.xml:include_volContent1-classi_code_head" />
