<?xml version="1.0" encoding="UTF-8"?>
<configuration type="jpvolume">
    <structure>
        <!-- Object Parent -->
        <element name="parents" minOccurs='1' maxOccurs='1'>
            <mcrmetalinkid name="parent" class="MCRMetaLinkID" minOccurs='1' maxOccurs='1' />
        </element>
        <!-- Object Children -->
        <element name="children" minOccurs='0' maxOccurs='1'>
            <mcrmetalinkid name="child" class="MCRMetaLinkID" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <!-- Derivate Objects -->
        <element name="derobjects" minOccurs='0' maxOccurs='1'>
            <mcrmetalinkid name="derobject" class="MCRMetaLinkID" minOccurs='0' maxOccurs='unbounded' />
        </element>
    </structure>
    <metadata>
        <!-- 01 - Maintitle -->
        <element name="maintitles" minOccurs='1' maxOccurs='1'>
            <mcrmetalangtext name="maintitle" class="MCRMetaLangText" minOccurs='1' maxOccurs='10' length="1024" />
        </element>
        <!-- 02 - SubTitles -->
        <!-- types = addtitle | shorttitle | misc. -->
        <element name="subtitles" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="subtitle" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <!-- 03 - participantss -->
        <!-- for @types watch jportal_class_00000007 -->
        <element name="participants" minOccurs='0' maxOccurs='1'>
            <mcrmetalinkid name="participant" class="MCRMetaLinkID" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <!-- 05 - keywords  -->
        <element name="keywords" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="keyword" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <!-- type = issn | isbn | nbn | doi | misc. -->
        <element name="identis" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="identi" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <!-- added for VD17 -->
        <element name="people" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="person" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <element name="publicationNotes" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="publicationNote" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <element name="normedPubLocations" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="normedPubLocation" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <element name="footNotes" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="footNote" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <element name="bibEvidences" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="bibEvidence" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <element name="indexFields" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="indexField" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' length="1024" />
        </element>
        <!-- added for VD17 END -->
        <!-- type = short | long -->
        <element name="abstracts" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="abstract" class="MCRMetaLangText" minOccurs='0' maxOccurs='30' length="4096" />
        </element>
        <element name="collationNotes" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="collationNote" class="MCRMetaLangText" minOccurs='0' maxOccurs='30' length="4096" />
        </element>

        <element name="derivateLinks" minOccurs='0' maxOccurs='1'>
          <mcrmetaderivatelink name="derivateLink" class="MCRMetaDerivateLink" minOccurs='1' maxOccurs='unbounded' />
        </element>

        <!-- date -->
        <element name="dates" minOccurs='0' maxOccurs='1'>
            <mcrmetaiso8601date name="date" class="MCRMetaISO8601Date" minOccurs='1' maxOccurs='2' />
        </element>
        <!-- tradition / calendar extension -->
        <element name="traditions" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="tradition" class="MCRMetaLangText" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <!-- 14 - notes -->
        <element name="notes" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="note" class="MCRMetaLangText" minOccurs='0' maxOccurs='1' length="4096" />
        </element>
        <!-- dynamically generated classification -->
        <element name="volContentClassis1" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="volContentClassi1" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <element name="volContentClassis2" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="volContentClassi2" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <element name="volContentClassis3" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="volContentClassi3" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <element name="volContentClassis4" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="volContentClassi4" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <element name="volContentClassis5" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="volContentClassi5" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <element name="volContentClassis6" minOccurs='0' maxOccurs='1'>
            <mcrmetaclassification name="volContentClassi6" class="MCRMetaClassification" minOccurs='0' maxOccurs='unbounded' />
        </element>
        <element name="hidden_jpjournalsID" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="hidden_jpjournalID" class="MCRMetaLangText" minOccurs='0' maxOccurs='1' />
        </element>
        <element name="hidden_positions" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="hidden_position" class="MCRMetaLangText" minOccurs='0' maxOccurs='1' />
        </element>
        <!-- ================================================================================================= -->
    </metadata>
    <service>
        <!-- Service Dates -->
        <element name="servdates" minOccurs='0' maxOccurs='1'>
            <mcrmetaiso8601date name="servdate" class="MCRMetaISO8601Date" minOccurs='1' maxOccurs='unbounded' />
        </element>
        <!-- Service Flags -->
        <element name="servflags" minOccurs='0' maxOccurs='1'>
            <mcrmetalangtext name="servflag" class="MCRMetaLangText" minOccurs='1' maxOccurs='unbounded' length="256" />
        </element>
        <!-- Service ACLs -->
        <element name="servacls" minOccurs='0' maxOccurs='1'>
            <mcrmetaaccessrule name="servacl" class="MCRMetaAccessRule" minOccurs='1' maxOccurs='unbounded' length="512" />
        </element>
    </service>
</configuration>