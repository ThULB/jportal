<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MyCoReWebPage>

<MyCoReWebPage>
  
  <section i18n="component.swf.page.commit.acl.title" direction="ltr">
    <editor id="commit-acl-ltr">
      
      <source uri="{sourceUri}"/>
      <target type="servlet" name="MCRCheckCommitACLServlet" method="post" format="xml"/>
      <cancel url="{cancelUrl}"/>
      
      <validationMessage i18n="common.validationMessage"/>
      
      <components root="editor-acls-ltr" var="/service" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xlink="http://www.w3.org/1999/xlink"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <include uri="webapp:editor/imports-editor-acl-ltr.xml"/>
        <include uri="webapp:editor/imports-editor-acl-project.xml"/>
      </components>
    </editor>
  </section>
  
  <section i18n="component.swf.page.commit.acl.title" direction="rtl">
    <div align="right">
      <editor id="commit-acl-rtl">
        
        <source uri="{sourceUri}"/>
        <target type="servlet" name="MCRCheckCommitACLServlet" method="post" format="xml"/>
        <cancel url="{cancelUrl}"/>
        
        <validationMessage i18n="common.validationMessage"/>
        
        <components root="editor-acls-rtl" var="/service" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
          <include uri="webapp:editor/imports-editor-acl-rtl.xml"/>
          <include uri="webapp:editor/imports-editor-acl-project.xml"/>
        </components>
      </editor>
    </div>
  </section>
  
</MyCoReWebPage>
