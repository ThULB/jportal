<?xml version="1.0" encoding="UTF-8"?><MyCoReWebPage>
  <section title="Artikel bearbeiten" xml:lang="de"/>
  <section title="edit article" xml:lang="en"/>
  <section xml:lang="all">
    <p class="editorform">
      <editor id="editor-jparticle">
        <source uri="jportal_getParentID:{parentID}"/>
        <source uri="{sourceUri}"/>
        <cancel url="servlets/MCRJPortalEditorCancelServlet"/>

        <validationMessage>
          <label xml:lang="de">Eingabefehler: Bitte korrigieren Sie die markierten Felder.</label>
          <label xml:lang="en">Type error: Please correct the marked fields.</label>
        </validationMessage>

        <target type="servlet" name="MCRJPortalCheckCommitDataServlet" method="post" format="xml"/>
        <components xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" root="editor-jparticle" var="/mycoreobject">

          <include uri="webapp:editor/editor-jparticle.xml"/>
          <panel id="editor-jparticle">
            <cell row="1" col="1" anchor="NORTHWEST" ref="edit-jparticle"/>
          </panel>

        </components>
      </editor>
    </p>
  </section>
</MyCoReWebPage>