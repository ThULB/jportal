<?xml version="1.0" encoding="ISO-8859-1"?>
<imports xmlns:xhtml="http://www.w3.org/1999/xhtml">
  <!-- 
    <include uri="webapp:editor/imports-application.xml" />
  -->
  <!-- *** Text fields *** -->
  <textfield id="TFzeile10" width="10" maxlength="10" />
  <textfield id="TFzeile32" width="32" maxlength="32" />
  <textfield id="TFzeile64" width="48" maxlength="64" />
  <textfield id="TFzeile128" width="48" maxlength="128" />
  <textfield id="TFzeile1024" width="64" maxlength="1024" />
  <!-- *** Text area *** -->
  <textarea id="TFarea4096" width="48" height="3" maxlength="4096" />
  <!-- *** Language List *** -->
  <list id="Slang" width="60" type="dropdown">
    <item value="de">
      <label xml:lang="de">de</label>
    </item>
    <item value="en">
      <label xml:lang="de">en</label>
    </item>
  </list>
  <!-- *** Data for the buttons ** -->
  <panel id="TwoButtons">
    <cell col="1" row="1" anchor="WEST">
      <cancelButton width="100" i18n="buttons.cancel"/>
    </cell>
    <cell col="2" row="1" anchor="WEST">
      <submitButton width="100" i18n="buttons.save" />
    </cell>
  </panel>
  <!-- *** bemerkungen ** -->
  <panel id="Dnote">
    <hidden var="@class" default="MCRMetaLangText" />
    <hidden var="@notinherit" default="true" />
    <hidden var="@heritable" default="true" />
    <hidden var="note/@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFarea4096" var="note" />
  </panel>
  <text id="Lnote">
    <label xml:lang="de">
      <xhtml:span class="editorlabel">&#160;Bemerkung &#160;</xhtml:span>
    </label>
    <label xml:lang="en">
      <xhtml:span class="editorlabel">&#160;Note &#160;</xhtml:span>
    </label>
  </text>
  <helpPopup id="Hnote" width="400" height="250">
    <label xml:lang="de">Hier können Bemerkungen zum leichteren Auffinden des Objektes eingetragen werden.</label>
    <label xml:lang="en">You can write keywords in this area for to be found the object faster.</label>
  </helpPopup>

  <!-- *** Parents *** -->
  <panel id="Dparent">
    <hidden var="structure/parents/@class" default="MCRMetaLinkID" />
    <hidden var="structure/parents/@notinherit" default="false" />
    <hidden var="structure/parents/@heritable" default="false" />
    <hidden var="structure/parents/parent/@inherited" default="0" />
    <hidden var="structure/parents/parent/@xlink:type" default="locator" />
    <hidden var="structure/parents/parent/@label" />
    <cell col="1" row="1" anchor="WEST" ref="TFzeile64" var="structure/parents/parent/@xlink:href" />
  </panel>
  <text id="Lparent">
    <label xml:lang="de">
      <xhtml:span class="editorlabel">&#160;Oberobjekt&#160;</xhtml:span>
    </label>
    <label xml:lang="en">
      <xhtml:span class="editorlabel">&#160;Parent&#160;</xhtml:span>
    </label>
  </text>
  <helpPopup id="Hparent" width="400" height="250">
    <label xml:lang="de">Hier können Parents zum leichteren Auffinden des Objektes eingetragen werden.</label>
    <label xml:lang="en">You can write Parents in this area for to be found the object faster.</label>
  </helpPopup>

  <!-- *** Repeater *** -->
  <panel id="repeater10">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFzeile10" />
  </panel>

  <panel id="repeater32">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFzeile32" />
  </panel>

  <panel id="repeater64">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFzeile64" />
  </panel>

  <panel id="repeater128">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFzeile128" />
  </panel>

  <panel id="repeater1024">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFzeile1024" />
  </panel>

  <panel id="repeater4096">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="WEST" ref="TFarea4096" />
  </panel>

  <panel id="linkrepeater" lines="off">
    <hidden var="@inherited" default="0" />
    <hidden var="@type" default="locator" />
    <hidden var="@title" default="Person" />

    <hidden var="@href" default="" />
    <cell row="1" col="1" width="100%">
      <output var="@title" default="Person" />
      <text label="&#160;&#160;&#160;" />
    </cell>
    <cell row="1" col="2">
      <subselect id="sub.select1" type="webpage" href="indexpage?searchclass=person_sub" sortnr="6">
        <label xml:lang="de">Auswählen</label>
        <label xml:lang="en">Select</label>
      </subselect>
    </cell>
  </panel>
  <!-- *** End *** -->
</imports>