   <parsable targetfile="$INSTALL_PATH/webapps/WEB-INF/classes/mycore.properties.wcms" type="javaprop"/>
   <singlefile target="$INSTALL_PATH/webapps/WEB-INF/server-config.wsdd" src="config/server-config.wsdd.izpack" />
   <parsable targetfile="$INSTALL_PATH/webapps/WEB-INF/server-config.wsdd" type="javaprop"/>
  </pack>

