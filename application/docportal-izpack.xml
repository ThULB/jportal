 <guiprefs resizable="no" width="600" height="400">
  <laf name="liquid">
   <os family="unix"/>
  </laf>
  <laf name="looks">
   <os family="windows"/>
   <param name="variant" value="plasticXP"/>
  </laf>
 </guiprefs>

 <locale>
  <langpack iso3="deu"/>
  <langpack iso3="eng"/>
 </locale>

 <resources>
  <res id="InfoPanel.info_deu" src="readme-izpack-de.txt" />
  <res id="InfoPanel.info_eng" src="readme-izpack-en.txt" />
  <res id="LicencePanel.licence" src="license.txt" />
<!--
  <res id="Installer.image" src="documentations/GFX/install-logo.gif" />
-->
 </resources>

 <panels>
  <panel classname="HelloPanel" />
  <panel classname="InfoPanel" />
  <panel classname="LicencePanel" />
  <panel classname="TargetPanel" />
  <panel classname="PacksPanel" />
  <panel classname="InstallPanel" />
  <panel classname="FinishPanel" />
 </panels>

 <packs>

  <pack name="DocPortal Core Files" required="yes">
   <description>The core of the MyCoRe Sample Project DocPortal</description>
   <!-- flat files -->
   <file targetdir="$INSTALL_PATH" src="changelog.txt" />
   <file targetdir="$INSTALL_PATH" src="license.txt" />
   <file targetdir="$INSTALL_PATH" src="build.xml" />
   <!-- bin files UNIX -->
   <fileset targetdir="$INSTALL_PATH/bin" dir="bin" os="unix" >
    <include name="**/mycore.sh"/>
    <include name="**/listPlugins.sh"/>
    <include name="**/hsqldb*.sh"/>
    <include name="**/jetty*.sh"/>
    <include name="**/sqltool.rc"/>
    <include name="**/Save*.sh"/>
    <include name="**/SQLCount*.sh"/>
    <exclude name="**/SaveCron*.sh"/>
   </fileset>
   <parsable targetfile="$INSTALL_PATH/bin/SQLCountDB2.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/SQLCountHSQLDB.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/SQLCountMySQL.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/Save.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveContent.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveDerivate.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveList.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveUser.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/mycore.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/listPlugins.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/hsqldbadmin.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/hsqldbstart.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/hsqldbstop.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/jettystart.sh" os="unix" />
   <parsable targetfile="$INSTALL_PATH/bin/jettystop.sh" os="unix" />
   <executable targetfile="$INSTALL_PATH/bin/SQLCountDB2.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/SQLCountHSQLDB.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/SQLCountMySQL.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/Save.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/SaveContent.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/SaveDerivate.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/SaveList.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/SaveUser.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/mycore.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/listPlugins.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/hsqldbadmin.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/hsqldbstart.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/hsqldbstop.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/jettystart.sh" keep="true" stage="never" os="unix" failure="warn" />
   <executable targetfile="$INSTALL_PATH/bin/jettystop.sh" keep="true" stage="never" os="unix" failure="warn" />
   <!-- bin files Windows -->
   <fileset targetdir="$INSTALL_PATH/bin" dir="bin" os="windows" >
    <include name="**/mycore*.cmd"/>
    <include name="**/hsqldb*.cmd"/>
    <include name="**/jetty*.cmd"/>
    <include name="**/sqltool.rc"/>
    <include name="**/Save*.cmd"/>
    <include name="**/SQLCount*.cmd"/>
   </fileset>
   <fileset targetdir="$INSTALL_PATH/dostools/gnutools" dir="dostools/gnutools" os="windows" >
    <include name="**/*"/>
   </fileset>
   <parsable targetfile="$INSTALL_PATH/bin/SQLCountHSQLDB.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/Save.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveContent.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveDerivate.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveList.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/SaveUser.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/mycore.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/hsqldbadmin.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/hsqldbstart.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/hsqldbstop.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/jettystart.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/bin/jettystop.cmd" os="windows" />
   <!-- schortcut -->
   <fileset targetdir="$INSTALL_PATH/dostools" dir="dostools" os="windows" >
    <include name="**/mklink_install.cmd"/>
    <include name="**/rmlink_install.cmd"/>
   </fileset>
   <fileset targetdir="$INSTALL_PATH/dostools/Shortcut" dir="dostools/Shortcut" os="windows" >
    <include name="**/*"/>
   </fileset>
   <parsable targetfile="$INSTALL_PATH/dostools/mklink_install.cmd" os="windows" />
   <parsable targetfile="$INSTALL_PATH/dostools/rmlink_install.cmd" os="windows" />
   <executable targetfile="$INSTALL_PATH/dostools/mklink_install.cmd" keep="true" stage="postinstall" os="windows" failure="warn" />
   <executable targetfile="$INSTALL_PATH/dostools/rmlink_install.cmd" keep="true" stage="uninstall" os="windows" failure="warn" />
   <!-- config files -->
   <fileset targetdir="$INSTALL_PATH/config" dir="config" >
    <include name="**/ContentStoreSelectionRules.xml"/>
    <include name="**/FileContentTypes.xml"/>
    <include name="**/MCRRESUMPTIONTOKEN.hbm.xml"/>
    <include name="**/datamodel*"/>
    <include name="**/editor*"/>
    <include name="**/imports*"/>
    <include name="**/mycore*"/>
    <include name="**/message*"/>
    <include name="**/Search*"/>
    <include name="**/searchfields.xml"/>
    <include name="**/reservation.xml"/>
    <include name="**/user/group_*"/>
    <include name="**/permissions.xml"/>
    <include name="**/user_*"/>
    <exclude name="**/mycore.properties.private*"/>
   </fileset>
   <singlefile target="$INSTALL_PATH/config/mycore.properties.private" src="config/mycore.properties.private.izpack" />
   <singlefile target="$INSTALL_PATH/webapps/WEB-INF/classes/mycore.properties.private" src="config/mycore.properties.private.izpack" />
   <singlefile target="$INSTALL_PATH/config/build.properties" src="config/build.properties" />
   <parsable targetfile="$INSTALL_PATH/config/mycore.properties.private" type="javaprop" />
   <!-- lib files -->
   <fileset targetdir="$INSTALL_PATH/lib" dir="lib" >
    <include name="**/*" />
   </fileset>
   <!-- classes files -->
   <fileset targetdir="$INSTALL_PATH/classes" dir="classes" >
    <include name="**/*" />
   </fileset>
   <!-- stylesheets files -->
   <fileset targetdir="$INSTALL_PATH/stylesheets" dir="stylesheets" >
    <include name="**/*" />
   </fileset>
   <!-- schema files -->
   <fileset targetdir="$INSTALL_PATH/schema" dir="schema" >
    <include name="**/*" />
   </fileset>
   <!-- webpages files -->
   <fileset targetdir="$INSTALL_PATH/webpages" dir="webpages" >
    <include name="**/*" />
   </fileset>
  </pack>

  <pack name="Web Application" required="yes" preselected="yes">
   <depends packname="DocPortal Core Files" />
   <description>The WEB application of the MyCoRe Sample Project DocPortal</description>
   <!-- the modules -->
   <fileset targetdir="$INSTALL_PATH/modules" dir="modules" >
    <include name="**/*" />
   </fileset>
   <!-- the webapps data -->
   <fileset targetdir="$INSTALL_PATH/webapps" dir="webapps" >
    <include name="**/*" />
    <exclude name="**/WEB-INF/classes/mycore.properties.private*"/>
    <exclude name="**/WEB-INF/classes/mycore.properties.wcms*"/>
    <exclude name="**/WEB-INF/server-config.wsdd*"/>
   </fileset>
   <singlefile target="$INSTALL_PATH/webapps/WEB-INF/classes/mycore.properties.private" src="config/mycore.properties.private.izpack" />
   <parsable targetfile="$INSTALL_PATH/webapps/WEB-INF/classes/mycore.properties.private" type="javaprop"/>
