<?xml version="1.0" encoding="utf-8"?>
<configuration type="person">
	<structure>
		<!-- Object Children -->
		<element name="children" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="false">
			<mcrmetalinkid name="child" class="MCRMetaLinkID" minOccurs='1'
				maxOccurs='unbounded'/>
		</element>
		<!-- Object Parent -->
		<element name="parents" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="false">
			<mcrmetalinkid name="parent" class="MCRMetaLinkID" minOccurs='1'
				maxOccurs='1'/>
		</element>
		<!-- Derivate Objects -->
		<element name="derobjects" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="false">
			<mcrmetalinkid name="derobject" class="MCRMetaLinkID" minOccurs='1'
				maxOccurs='unbounded'/>
		</element>
	</structure>
	<metadata>
		<!-- 01 - Ansetzungsform: Name -->
		<element name="def.heading" minOccurs='1' maxOccurs='1' parasearch="true"
			textsearch="true">
			<mcrmetalangtext name="heading" class="MCRMetaLangText" minOccurs='1'
				maxOccurs='1' length="256"/>
		</element>
		<!-- 02 - Weitere Namensformen -->
		<element name="def.alternative" minOccurs='0' maxOccurs='1'
			parasearch="true" textsearch="true">
			<mcrmetalangtext name="alternative" class="MCRMetaLangText"
				minOccurs='1' maxOccurs='unbounded' length="256"/>
		</element>
		<!-- 03 - Adelstitel -->
		<element name="def.peerage" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="true">
			<mcrmetalangtext name="peerage" class="MCRMetaLangText" minOccurs='1'
				maxOccurs='1' length="1024"/>
		</element>
		<!-- 04 - Ã„mter und Funktionen -->
		<element name="def.role" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="true">
			<mcrmetalangtext name="role" class="MCRMetaLangText" minOccurs='1'
				maxOccurs='unbounded' length="1024"/>
		</element>
		<!-- 05 - Lebens- und Wirkungsraum -->
		<element name="def.placeOfActivity" minOccurs='0' maxOccurs='1'
			parasearch="true" textsearch="true">
			<mcrmetalangtext name="placeOfActivity" class="MCRMetaLangText"
				minOccurs='1' maxOccurs='unbounded' length="1024"/>
		</element>
		<!-- 06 - Geburtsdatum -->
		<element name="def.dateOfBirth" minOccurs='0' maxOccurs='1'
			parasearch="true" textsearch="false">
			<mcrmetaiso8601date name="dateOfBirth" class="MCRMetaISO8601Date"
				minOccurs='1' maxOccurs='1'/>
		</element>
		<!-- 07 - Geburtsort -->
		<element name="def.placeOfBirth" minOccurs='0' maxOccurs='1'
			parasearch="true" textsearch="true">
			<mcrmetalangtext name="placeOfBirth" class="MCRMetaLangText"
				minOccurs='1' maxOccurs='1' length="256"/>
		</element>
		<!-- 08 - Sterbedatum -->
		<element name="def.dateOfDeath" minOccurs='0' maxOccurs='1'
			parasearch="true" textsearch="false">
			<mcrmetaiso8601date name="dateOfDeath" class="MCRMetaISO8601Date"
				minOccurs='1' maxOccurs='1'/>
		</element>
		<!-- 09 - Sterbeort -->
		<element name="def.placeOfDeath" minOccurs='0' maxOccurs='1'
			parasearch="true" textsearch="true">
			<mcrmetalangtext name="placeOfDeath" class="MCRMetaLangText"
				minOccurs='1' maxOccurs='1' length="256"/>
		</element>
		<!-- 10 - Bemerkungen -->
		<element name="def.note" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="true">
			<mcrmetalangtext name="note" class="MCRMetaLangText" minOccurs='1'
				maxOccurs='1' length="1024"/>
		</element>
	</metadata>
	<service>
		<!-- Service Dates -->
		<element name="servdates" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="false">
			<mcrmetaiso8601date name="servdate" class="MCRMetaISO8601Date"
				minOccurs='1' maxOccurs='unbounded'/>
		</element>
		<!-- Service Flags -->
		<element name="servflags" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="false">
			<mcrmetalangtext name="servflag" class="MCRMetaLangText" minOccurs='1'
				maxOccurs='unbounded' length="256"/>
		</element>
		<!-- Service ACLs -->
		<element name="servacls" minOccurs='0' maxOccurs='1' parasearch="true"
			textsearch="false">
			<mcrmetaaccessrule name="servacl" class="MCRMetaAccessRule"
				minOccurs='1' maxOccurs='unbounded' length="512"/>
		</element>
	</service>
</configuration>