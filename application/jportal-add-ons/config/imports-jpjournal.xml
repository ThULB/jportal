<?xml version="1.0" encoding="ISO-8859-1"?>
<imports>
            <include uri="webapp:editor/imports-jpcommon.xml" />
            <include uri="webapp:editor/imports-jplabelsHelptexts.xml" />            
      
      <panel id="globalImport.jpjournal" lines="on">
            <hidden var="ID" />
            <hidden var="label" />
            <hidden var="metadata/@lang" default="de" />
			<hidden var="service/servdates" descendants="true"/>
			<hidden var="service/servacls" descendants="true"/>
			<hidden var="service/servflags" descendants="true"/>		  
            <!-- required fields ......................................... -->
            <!-- maintitle -->
            <cell row="10" col="1" anchor="NORTHWEST" ref="maintitle_label" />
            <cell row="10" col="2" anchor="NORTHWEST" ref="maintitle_input" />
            <cell row="10" col="3" anchor="WEST" ref="maintitle_help" />
            <!-- subtitle -->
            <cell row="20" col="1" anchor="NORTHWEST" ref="subtitle_label" />
            <cell row="20" col="2" anchor="NORTHWEST" ref="subtitle_input" />
            <cell row="20" col="3" anchor="NORTHWEST" ref="subtitle_help" />
            <!-- mainedi -->
            <cell row="30" col="1" anchor="NORTHWEST" ref="mainedi_label_bold" />
            <cell row="30" col="2" anchor="NORTHWEST" ref="mainedi_input" />
            <cell row="30" col="3" anchor="WEST" ref="mainedi_help" />
            <!-- subedi -->
            <cell row="40" col="1" anchor="NORTHWEST" ref="subedi_label" />
            <cell row="40" col="2" anchor="NORTHWEST" ref="subedi_input" />
            <cell row="40" col="3" anchor="WEST" ref="subedi_help" />
            <!-- mainauthor -->
            <cell row="50" col="1" anchor="NORTHWEST" ref="author_label" />
            <cell row="50" col="2" anchor="NORTHWEST" ref="author_input" />
            <cell row="50" col="3" anchor="WEST" ref="author_help" />
            <!-- contribu -->
            <cell row="70" col="1" anchor="NORTHWEST" ref="contribu_label" />
            <cell row="70" col="2" anchor="NORTHWEST" ref="contribu_input" />
            <cell row="70" col="3" anchor="WEST" ref="contribu_help" />
            <!-- date -->
            <cell row="75" col="1" anchor="NORTHWEST" ref="date_label" />
            <cell row="75" col="2" anchor="NORTHWEST" ref="date_input" />
            <!-- keyword -->
            <cell row="80" col="1" anchor="NORTHWEST" ref="keyword_label" />
            <cell row="80" col="2" anchor="NORTHWEST" ref="keyword_input" />
            <cell row="80" col="3" anchor="WEST" ref="keyword_help" />
            <!-- classi -->
            <cell row="90" col="1" anchor="NORTHWEST" ref="classi_label" />
            <cell row="90" col="2" anchor="NORTHWEST" ref="classi_input" />
            <cell row="90" col="3" anchor="WEST" ref="classi_help" />
            <!-- abstract -->
            <cell row="100" col="1" anchor="NORTHWEST" ref="abstract_label" />
            <cell row="100" col="2" anchor="NORTHWEST" ref="abstract_input" />
            <cell row="100" col="3" anchor="WEST" ref="abstract_help" />
            <!-- identi -->
            <cell row="130" col="1" anchor="NORTHWEST" ref="identi_label" />
            <cell row="130" col="2" anchor="NORTHWEST" ref="identi_input" />
            <cell row="130" col="3" anchor="WEST" ref="identi_help" />
            <!-- lang -->
            <cell row="140" col="1" anchor="NORTHWEST" ref="cont_lang_label_bold" />
            <cell row="140" col="2" anchor="NORTHWEST" ref="cont_lang_input" />
            <cell row="140" col="3" anchor="NORTHWEST" ref="cont_lang_help" />
            <!-- right -->
            <cell row="150" col="1" anchor="NORTHWEST" ref="right_label" />
            <cell row="150" col="2" anchor="NORTHWEST" ref="right_input" />
            <cell row="150" col="3" anchor="WEST" ref="right_help" />
            <!-- note -->
            <cell row="160" col="1" anchor="NORTHWEST" ref="note_label" />
            <cell row="160" col="2" anchor="NORTHWEST" ref="note_input" />
            <cell row="160" col="3" anchor="WEST" ref="note_help" />
            <!-- type -->
            <cell row="170" col="1" anchor="NORTHWEST" ref="type_label_bold" />
            <cell row="170" col="2" anchor="NORTHWEST" ref="type_input" />
            <cell row="170" col="3" anchor="NORTHWEST" ref="type_help" />
            <!-- predece -->
            <cell row="180" col="1" anchor="NORTHWEST" ref="predece_label" />
            <cell row="180" col="2" anchor="NORTHWEST" ref="predece_input" />
            <cell row="180" col="3" anchor="WEST" ref="predece_help" />
            <!-- successor -->
            <cell row="190" col="1" anchor="NORTHWEST" ref="successor_label" />
            <cell row="190" col="2" anchor="NORTHWEST" ref="successor_input" />
            <cell row="190" col="3" anchor="WEST" ref="successor_help" />
            <!-- save / cancel button -->
            <cell row="220" col="1" anchor="EAST"/>
            <cell row="220" col="2" anchor="EAST" ref="save" />
      </panel>
      <!-- panel definitions -->
      <!-- MainTitle ================================================================== -->
      <panel id="maintitle_input">
            <hidden var="metadata/maintitles/@class" default="MCRMetaLangText" />
            <hidden var="metadata/maintitles/@parasearch" default="true" />
            <hidden var="metadata/maintitles/@textsearch" default="true" />
            <hidden var="metadata/maintitles/@notinherit" default="false" />
            <hidden var="metadata/maintitles/@heritable" default="false" />
            <hidden var="metadata/maintitles/maintitle/@inherited" default="0" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="Slang" var="metadata/maintitles/maintitle/@lang" 
                  default="de" />
            <cell col="1" row="2" anchor="WEST" ref="TFarea4096" var="metadata/maintitles/maintitle" />
      </panel>
      <!-- end: MainTitle -->
      <!-- SubTitle ================================================================== -->
      <panel id="subtitle_input">
            <hidden var="metadata/subtitles/@class" default="MCRMetaLangText" />
            <hidden var="metadata/subtitles/@parasearch" default="true" />
            <hidden var="metadata/subtitles/@textsearch" default="true" />
            <hidden var="metadata/subtitles/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/subtitles/subtitle" >
                  <repeater min="0" max="20" anchor="NORTHWEST" ref="subtitle_input_repeater" />
            </cell>
      </panel>
      <panel id="subtitle_input_repeater">
            <cell col="1" row="1" anchor="NORTHWEST" ref="subtitle_type" var="@type" />		  
            <cell col="1" row="2" anchor="NORTHWEST" ref="Slang" var="@lang" default="de" />		  
            <cell col="1" row="3" anchor="NORTHWEST" ref="TFarea4096"  />
      </panel>
      <!-- end: SubTitle -->
      <!-- mainedi ================================================================== -->
      <panel id="mainedi_input">
            <hidden var="metadata/mainedis/@class" default="MCRMetaLinkID" />
            <hidden var="metadata/mainedis/@parasearch" default="true" />
            <hidden var="metadata/mainedis/@textsearch" default="true" />
            <hidden var="metadata/mainedis/@notinherit" default="false" />
            <hidden var="metadata/mainedis/@heritable" default="false" />

		    <cell row="1" col="1" anchor="EAST" var="metadata/mainedis/mainedi">
	         <repeater min="1" max="10">
	           <panel lines="off">
				 <hidden var="@inherited" default="0" />
				 <hidden var="@type" default="locator" />
		         <hidden var="@title" default="Herausgeber" />			   
				 <hidden var="@label" />
				 <hidden var="@href" />			
				   
				 <cell row="1" col="1" anchor="NORTHWEST" ref="getPersonAndInst"/>

	           </panel>
	         </repeater>
	       </cell>	  		  
      </panel>
      <!-- end: mainedi -->
      <!-- subedi ================================================================== -->
      <panel id="subedi_input">
            <hidden var="metadata/subedis/@class" default="MCRMetaLinkID" />
            <hidden var="metadata/subedis/@parasearch" default="true" />
            <hidden var="metadata/subedis/@textsearch" default="true" />
            <hidden var="metadata/subedis/@notinherit" default="false" />
            <hidden var="metadata/subedis/@heritable" default="false" />
			  
		    <cell row="1" col="2" anchor="EAST" var="metadata/subedis/subedi">
	         <repeater min="1" max="10">
	           <panel lines="off">
				 <hidden var="@inherited" default="0" />
				 <hidden var="@type" default="locator" />
	             <hidden var="@title" default="Mitherausgeber" />			   
				 <hidden var="@label" />
				 <hidden var="@href" />			
				 <cell row="1" col="1" anchor="NORTHWEST" ref="getPersonAndInst"/>   
	           </panel>
	         </repeater>
	       </cell>	  			  
      </panel>
      <!-- end: subedi -->
      <!-- author ================================================================== -->
      <panel id="author_input">
        <hidden var="metadata/authors/@class" default="MCRMetaLinkID" sortnr="3" />
        <hidden var="metadata/authors/@parasearch" default="true" sortnr="3" />
        <hidden var="metadata/authors/@textsearch" default="true" sortnr="3" />
	    <cell col="1" row="1" anchor="EAST" var="metadata/authors/author" sortnr="3" >
         <repeater min="1" max="10">
           <panel lines="off">
			 <hidden var="@inherited" default="0" />
			 <hidden var="@type" default="locator" />
			 <hidden var="@label" />
			 <hidden var="@href" />			
             <cell col="1" row="1" anchor="NORTHWEST" ref="author_input_repeater_type" var="@title" />
			 <cell row="2" col="1" anchor="NORTHWEST" ref="getPersonAndInst"/>
           </panel>		 
         </repeater>
       </cell>	  
	  </panel>
      <!-- end: author -->
      <!-- contribu ================================================================== -->
      <panel id="contribu_input">
            <hidden var="metadata/contribus/@class" default="MCRMetaLinkID" />
            <hidden var="metadata/contribus/@parasearch" default="true" />
            <hidden var="metadata/contribus/@textsearch" default="true" />
            <hidden var="metadata/contribus/@notinherit" default="false" />
            <hidden var="metadata/contribus/@heritable" default="false" />
		    <cell col="1" row="1" anchor="EAST" var="metadata/contribus/contribu" >
	         <repeater min="1" max="10">
	           <panel lines="off">
				 <hidden var="@inherited" default="0" />
				 <hidden var="@type" default="locator" />
				 <hidden var="@label" />
				 <hidden var="@href" />			
	             <cell col="1" row="1" anchor="NORTHWEST" ref="contribu_input_repeater_type" var="@title" />			   
				 <cell row="2" col="1" anchor="NORTHWEST" ref="getPersonAndInst"/>
	           </panel>
	         </repeater>
	       </cell>	  
	  </panel>
      <!-- end: contribu -->
      <!-- keyword ================================================================== -->
      <panel id="keyword_input">
            <hidden var="metadata/keywords/@class" default="MCRMetaLangText" />
            <hidden var="metadata/keywords/@parasearch" default="true" />
            <hidden var="metadata/keywords/@textsearch" default="true" />
            <hidden var="metadata/keywords/@heritable" default="true" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="Slang" var="metadata/keywords/keyword/@lang" ordernr="1" default="de" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile128" var="metadata/keywords/keyword" ordernr="1" />
      </panel>
      <!-- end: keyword -->
      <!-- classi ================================================================== -->
      <panel id="classi_input">
            <hidden var="metadata/classis/@class" default="MCRMetaClassification" />
            <hidden var="metadata/classis/@parasearch" default="true" />
            <hidden var="metadata/classis/@textsearch" default="false" />
            <hidden var="metadata/classis/@notinherit" default="false" />
            <hidden var="metadata/classis/@heritable" default="true" />
            <cell col="1" row="1" anchor=" NORTHWEST" var="metadata/classis/classi" ordernr="18">
                  <repeater min="0" max="10" anchor="NORTHWEST" ref="classi_input_repeater" />
            </cell>
      </panel>
      <panel id="classi_input_repeater">
            <hidden var="@inherited" default="0" />
            <hidden var="@classid" default="jportal_class_00000003" />
            <cell col="1" row="1" anchor=" NORTHWEST" ref="Cclassi" var="@categid" ordernr="18" />
      </panel>
      <list id="Cclassi" width="300" type="dropdown">
		    <include uri="classification:editor:-1:children:jportal_class_00000003" />		  
      </list>
      <!-- end: classi -->
      <!-- abstract ================================================================== -->
      <panel id="abstract_input">
            <hidden var="metadata/abstracts/@class" default="MCRMetaLangText" />
            <hidden var="metadata/abstracts/@parasearch" default="true" />
            <hidden var="metadata/abstracts/@textsearch" default="true" />
            <hidden var="metadata/abstracts/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/abstracts/abstract" ordernr="1" >
                  <repeater min="0" max="3" anchor="NORTHWEST" ref="abstract_input_repeater" />
            </cell>
      </panel>
      <panel id="abstract_input_repeater">
            <cell col="1" row="1" anchor="NORTHWEST" ref="abstract_type" var="@type" ordernr="1" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="Slang" var="@lang" ordernr="1" default="de" />
            <cell col="1" row="3" anchor="NORTHWEST" ref="TFarea4096" var="" ordernr="1" />
      </panel>
      <!-- end: abstract -->
      <!-- date ================================================================== -->
      <panel id="date_input">
            <hidden var="metadata/dates/@class" default="MCRMetaISO8601Date" />
            <hidden var="metadata/dates/@parasearch" default="true" />
            <hidden var="metadata/dates/@textsearch" default="true" />
            <hidden var="metadata/dates/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/dates/date" ordernr="1" >
                  <repeater min="0" max="2" anchor="NORTHWEST" ref="date_input_repeater" />
            </cell>
      </panel>
      <panel id="date_input_repeater">
            <cell col="1" row="1" anchor="NORTHWEST" ref="date_type" var="@type" ordernr="1" />		  
            <cell col="1" row="3" anchor="NORTHWEST" ref="TFzeile10" var="" ordernr="1" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="date_howToUse" />
      </panel>
      <!-- end: date -->
      <!-- identi ================================================================== -->
      <panel id="identi_input">
            <hidden var="metadata/identis/@class" default="MCRMetaLangText" />
            <hidden var="metadata/identis/@parasearch" default="true" />
            <hidden var="metadata/identis/@textsearch" default="true" />
            <hidden var="metadata/identis/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/identis/identi" ordernr="1" >
                  <repeater min="0" max="10" anchor="NORTHWEST" ref="identi_input_repeater" />
            </cell>
      </panel>
      <panel id="identi_input_repeater">
            <cell col="1" row="1" anchor="NORTHWEST" ref="identi_type" var="@type" ordernr="1" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile1024" var="" ordernr="1" />
      </panel>
      <!-- end: identi -->
      <!-- Lang ================================================================== -->
      <panel id="cont_lang_input">
            <hidden var="metadata/languages/@class" default="MCRMetaClassification" />
            <hidden var="metadata/languages/@parasearch" default="true" />
            <hidden var="metadata/languages/@textsearch" default="false" />
            <hidden var="metadata/languages/@notinherit" default="false" />
            <hidden var="metadata/languages/@heritable" default="false" />
            <hidden var="metadata/languages/language/@inherited" default="0" />
            <hidden var="metadata/languages/language/@classid" default="jportal_class_00000004" />
            <cell col="1" row="1" anchor=" NORTHWEST" ref="CLanguage" var="metadata/languages/language/@categid" />
      </panel>
      <list id="CLanguage" width="300" type="dropdown">
            <item value="">
                  <label xml:lang="de">
                  </label>
            </item>
		    <include uri="classification:editor:-1:children:jportal_class_00000004" />		  
      </list>
      <!-- end: Lang -->
      <!-- right ================================================================== -->
      <panel id="right_input">
            <hidden var="metadata/rights/@class" default="MCRMetaLangText" />
            <hidden var="metadata/rights/@parasearch" default="true" />
            <hidden var="metadata/rights/@textsearch" default="true" />
            <hidden var="metadata/rights/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="Slang" var="metadata/rights/right/@lang" ordernr="1" default="de" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile128" var="metadata/rights/right" ordernr="1" />
      </panel>
      <!-- end: right -->
      <!-- note ================================================================== -->
      <panel id="note_input">
            <hidden var="metadata/notes/@class" default="MCRMetaLangText" />
            <hidden var="metadata/notes/@parasearch" default="true" />
            <hidden var="metadata/notes/@textsearch" default="true" />
            <hidden var="metadata/notes/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="Slang" var="metadata/notes/note/@lang" ordernr="1" default="de" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="TFarea4096" var="metadata/notes/note" ordernr="1" />
      </panel>
      <!-- end: note -->
      <!-- type ================================================================== -->
      <panel id="type_input">
            <hidden var="metadata/types/@class" default="MCRMetaClassification" />
            <hidden var="metadata/types/@parasearch" default="true" />
            <hidden var="metadata/types/@textsearch" default="false" />
            <hidden var="metadata/types/@notinherit" default="false" />
            <hidden var="metadata/types/@heritable" default="false" />
            <hidden var="metadata/types/type/@inherited" default="0" />
            <hidden var="metadata/types/type/@classid" default="jportal_class_00000005" />
            <cell col="1" row="1" anchor=" NORTHWEST" ref="Ctype" var="metadata/types/type/@categid" ordernr="18" />
      </panel>
      <list id="Ctype" width="300" type="dropdown">
		    <include uri="classification:editor:-1:children:jportal_class_00000005" />		  
      </list>
      <!-- end: type -->
      <!-- predece ================================================================== -->
      <panel id="predece_input">
            <hidden var="metadata/predeces/@class" default="MCRMetaLink" />
            <hidden var="metadata/predeces/@parasearch" default="true" />
            <hidden var="metadata/predeces/@textsearch" default="true" />
            <hidden var="metadata/predeces/@notinherit" default="false" />
            <hidden var="metadata/predeces/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/predeces/predece">
                  <repeater min="0" max="10" anchor="NORTHWEST" ref="predece_input_repeater" />
            </cell>
      </panel>
      <panel id="predece_input_repeater">
            <hidden var="@inherited" default="0" />
            <hidden var="@type" default="locator" />
            <cell col="1" row="1" anchor="NORTHEAST" >
                  <text>
                        <label xml:lang="de">Name:</label>
                  </text>
            </cell>
            <cell col="2" row="1" anchor="NORTHWEST" var="@title">
                  <textfield id="predece_input_repeater_nameLink" width="64" maxlength="128" autofill="Name des Linkes" />
            </cell>
            <cell col="1" row="2" anchor="NORTHEAST" >
                  <text>
                        <label xml:lang="de">Link:</label>
                  </text>
            </cell>
            <cell col="2" row="2" anchor="NORTHWEST" var="@href" >
                  <textfield id="predece_input_repeater_valueLink" width="64" maxlength="128" autofill="www.link.de" />
            </cell>
      </panel>
      <!-- end: predece -->
      <!-- successor ================================================================== -->
      <panel id="successor_input">
            <hidden var="metadata/successors/@class" default="MCRMetaLink" />
            <hidden var="metadata/successors/@parasearch" default="true" />
            <hidden var="metadata/successors/@textsearch" default="true" />
            <hidden var="metadata/successors/@notinherit" default="false" />
            <hidden var="metadata/successors/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" ordernr="3" var="metadata/successors/successor">
                  <repeater min="0" max="10" anchor="NORTHWEST" ref="successor_input_repeater" />
            </cell>
      </panel>
      <panel id="successor_input_repeater">
            <hidden var="@inherited" default="0" />
            <hidden var="@type" default="locator" />
            <cell col="1" row="1" anchor="NORTHEAST" >
                  <text>
                        <label xml:lang="de">Name:</label>
                  </text>
            </cell>
            <cell col="2" row="1" anchor="NORTHWEST" var="@title">
                  <textfield id="successor_input_repeater_nameLink" width="64" maxlength="128" autofill="Name des Linkes" />
            </cell>
            <cell col="1" row="2" anchor="NORTHEAST" >
                  <text>
                        <label xml:lang="de">Link:</label>
                  </text>
            </cell>
            <cell col="2" row="2" anchor="NORTHWEST" var="@href" >
                  <textfield id="successor_input_repeater_valueLink" width="64" maxlength="128" autofill="www.link.de" />
            </cell>
      </panel>
      <!--date types =======================================================================-->
      <list id="date_type" type="dropdown">
            <item value="published">
                  <label xml:lang="de">Erschienen</label>
            </item>
            <item value="published_from">
                  <label xml:lang="de">Erschienen von</label>
            </item>
            <item value="published_until">
                  <label xml:lang="de">Erschienen bis</label>
            </item>	    
      </list>	
</imports>