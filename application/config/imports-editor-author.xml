<!-- *** NAME *** -->

  <panel id="DPName">
    <hidden var="metadata/names/@class" default="MCRMetaPersonName"/>
    <hidden var="metadata/names/@notinherit" default="false"/>
    <hidden var="metadata/names/@heritable" default="true"/>
    <hidden var="metadata/names/name/@inherited" default="0"/>
    <hidden var="metadata/names/name/@lang" default="de"/>
    <cell col="1" row="1" anchor="NORTHWEST" ref="DPNameSurname" />
    <cell col="2" row="1" anchor="NORTHWEST" ref="APNameSurname" />
    <cell col="1" row="2" anchor="NORTHWEST" ref="DPNameFirstname" />
    <cell col="2" row="2" anchor="NORTHWEST" ref="APNameFirstname" />
    <cell col="1" row="3" anchor="NORTHWEST" ref="DPNameAcademic" />
    <cell col="2" row="3" anchor="NORTHWEST" ref="APNameAcademic" />
    <cell col="1" row="4" anchor="NORTHWEST" ref="DPNamePeerage" />
    <cell col="2" row="4" anchor="NORTHWEST" ref="APNamePeerage" />
    <cell col="1" row="5" anchor="NORTHWEST" ref="DPNamePrefix" />
    <cell col="2" row="5" anchor="NORTHWEST" ref="APNamePrefix" />
  </panel>

  <panel id="DPNameSurname">
    <cell col="1" row="1" anchor="NORTHWEST" var="metadata/names/name/surname" sortnr="1">
      <textfield id="ZPNameSurname" width="48" maxlength="128">
        <condition id="CoPNameSurname" required="true">
          <label xml:lang="de">Bitte tragen Sie den Nachnamen ein!</label>
          <label xml:lang="en">Please enter the surname!</label>
        </condition>
      </textfield>
    </cell>
  </panel>

  <panel id="DPNameFirstname">
    <cell col="1" row="2" anchor="NORTHWEST" var="metadata/names/name/firstname" sortnr="1">
      <textfield id="ZPNameFirstname" width="48" maxlength="128">
        <condition id="CoPNameFirstname" required="true">
          <label xml:lang="de">Bitte tragen Sie den Vornamen ein!</label>
          <label xml:lang="en">Please enter the firstname!</label>
        </condition>
      </textfield>
    </cell>
  </panel>

  <panel id="DPNameAcademic">
    <cell col="1" row="1" anchor="NORTHWEST" ref="TFzeile32" var="metadata/names/name/academic" sortnr="1"/>
  </panel>

  <panel id="DPNamePeerage">
    <cell col="1" row="1" anchor="NORTHWEST" ref="TFzeile32" var="metadata/names/name/peerage" sortnr="1"/>
  </panel>

  <panel id="DPNamePrefix">
