    <cell col="2" row="1" anchor="NORTHWEST" var="metadata/females/female" ref="SFemale" sortnr="2" />
  </panel>

<!-- ** Data for the ORIGIN *** -->

  <panel id="DOrigin">
    <hidden var="metadata/institutions/@class" default="MCRMetaClassification"/>
    <hidden var="metadata/institutions/@notinherit" default="false"/>
    <hidden var="metadata/institutions/@heritable" default="true"/>
    <cell col="1" row="1" anchor="WEST" var="metadata/institutions/institution">
      <repeater min="1" max="5" ref="DOriginRep" pos="right"/>
    </cell>
  </panel>

  <panel id="DOriginRep">
    <hidden var="@inherited" default="0"/>
    <hidden var="@classid" default="DocPortal_class_00000002"/>
    <cell col="1" row="1" anchor="WEST" ref="COriginPers" var="@categid" sortnr="3"/>
  </panel>

<!-- *** Data for the ADDRESS ** -->
 
  <panel id="DAddress">
    <hidden var="metadata/addresses/@class" default="MCRMetaAddress"/>
    <hidden var="metadata/addresses/@notinherit" default="false"/>
    <hidden var="metadata/addresses/@heritable" default="true"/>
    <hidden var="metadata/addresses/address/@inherited" default="0"/>
    <hidden var="metadata/addresses/address/@lang" default="de"/>
    <cell col="1" row="1" anchor="NORTHWEST" ref="Scountry" var="metadata/addresses/address/country" sortnr="4"/>
    <cell col="2" row="1" anchor="NORTHWEST" ref="AAddressCountry"/>
    <cell col="1" row="2" anchor="NORTHWEST" ref="Sstate" var="metadata/addresses/address/state" sortnr="4"/>
    <cell col="2" row="2" anchor="NORTHWEST" ref="AAddressState"/>
    <cell col="1" row="3" anchor="NORTHWEST" ref="TFzeile10" var="metadata/addresses/address/zipcode" sortnr="4"/>
    <cell col="2" row="3" anchor="NORTHWEST" ref="AAddressZIP"/>
    <cell col="1" row="4" anchor="NORTHWEST" ref="TFzeile128" var="metadata/addresses/address/city" sortnr="4"/>
    <cell col="2" row="4" anchor="NORTHWEST" ref="AAddressCity"/>
    <cell col="1" row="5" anchor="NORTHWEST" ref="TFzeile128" var="metadata/addresses/address/street" sortnr="4"/>
    <cell col="2" row="5" anchor="NORTHWEST" ref="AAddressStreet"/>
    <cell col="1" row="6" anchor="NORTHWEST" ref="TFzeile10" var="metadata/addresses/address/number" sortnr="4"/>
    <cell col="2" row="6" anchor="NORTHWEST" ref="AAddressNumber"/>
  </panel>

<!-- *** Data for the PHONE ** -->

  <panel id="DPhone">
    <hidden var="metadata/phones/@class" default="MCRMetaLangText"/>
