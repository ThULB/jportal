<?xml version="1.0" encoding="iso-8859-1"?>
<configuration type="author">
      <structure>
            <!-- Object Parent -->
            <element name="parents" minOccurs='1' maxOccurs='1' parasearch="false" textsearch="false">
                  <mcrmetalinkid name="parent" class="MCRMetaLinkID" minOccurs='1' maxOccurs='unbounded' />
            </element>
      </structure>
      <metadata>
            <!-- 01 - Person Name -->
            <element name="names" minOccurs='1' maxOccurs='1' parasearch="true" textsearch="true">
                  <mcrmetapersonname name="name" class="MCRMetaPersonName" minOccurs='1' maxOccurs='1' length="128"/>
            </element>
            <!-- 01 - Additional person name(s) -->		  
		    <element name="addnames" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="true">
                  <mcrmetapersonname name="name" class="MCRMetaPersonName" minOccurs='0' maxOccurs='unbounded' length="128"/>
            </element>
            <!-- 02 - Gender -->
            <!-- male | female | neuter | unknown -->
            <element name="genders" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetalangtext name="gender" class="MCRMetaLangText" minOccurs='1' maxOccurs='1' length="32" />
            </element>            
            <!-- 04 - Address -->
            <element name="addresses" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetaaddress name="address" class="MCRMetaAddress" minOccurs='1' maxOccurs='unbounded' length="64"/>
            </element>
            <!-- 05 - Phones -->
            <element name="phones" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetalangtext name="phone" class="MCRMetaLangText" minOccurs='1' maxOccurs='unbounded' length="32" />
            </element>
            <!-- 06 - Dates -->
            <element name="dates" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetadate name="date" class="MCRMetaDate" minOccurs='1' maxOccurs='unbounded'/>
            </element>
            <!-- 06 - Date of death -->
            <element name="datesofdeath" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetadate name="dateofdeath" class="MCRMetaDate" minOccurs='1' maxOccurs='1'/>
            </element>		  
            <!-- 07 - prof -->
            <element name="profs" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="true">
                  <mcrmetalangtext name="prof" class="MCRMetaLangText" minOccurs='1' maxOccurs='unbounded' length="128" />
            </element>
            <!-- 08 - profcl -->
            <element name="profcls" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetaclassification name="profcl" class="MCRMetaClassification" minOccurs='1' maxOccurs='unbounded'/>
            </element>
            <!-- 09 - National -->
            <element name="nationals" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetaclassification name="national" class="MCRMetaClassification" minOccurs='1' maxOccurs='unbounded'/>
            </element>
            <!-- 10 - URL -->
            <element name="urls" minOccurs='0' maxOccurs='1' parasearch="false" textsearch="false">
                  <mcrmetalink name="url" class="MCRMetaLink" minOccurs='1' maxOccurs='unbounded'/>
            </element>
            <!-- 11 - eMail -->
            <element name="emails" minOccurs='0' maxOccurs='1' parasearch="false" textsearch="false">
                  <mcrmetalangtext name="email" class="MCRMetaLangText" minOccurs='1' maxOccurs='unbounded' length="128"/>
            </element>
            <!-- 13 - Note -->
            <element name="notes" minOccurs='0' maxOccurs='1' parasearch="false" textsearch="false">
                  <mcrmetalangtext name="note" class="MCRMetaLangText" minOccurs='1' maxOccurs='unbounded' length="4096"/>
            </element>
            <!-- 14 - pub -->
            <element name="pubs" minOccurs='0' maxOccurs='1' parasearch="false" textsearch="false">
                  <mcrmetalangtext name="pub" class="MCRMetaXML" minOccurs='1' maxOccurs='unbounded' length="4096"/>
            </element>
      </metadata>
      <service>
            <!-- Service Dates -->
            <element name="servdates" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetadate name="servdate" class="MCRMetaDate" minOccurs='1' maxOccurs='unbounded'/>
            </element>
            <!-- Service Flags -->
            <element name="servflags" minOccurs='0' maxOccurs='1' parasearch="true" textsearch="false">
                  <mcrmetalangtext name="servflag" class="MCRMetaLangText" minOccurs='1' maxOccurs='unbounded' length="256" />
            </element>
      </service>
</configuration>