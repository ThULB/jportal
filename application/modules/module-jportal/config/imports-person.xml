<?xml version="1.0" encoding="ISO-8859-1"?>
<components xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="editor.xsd">
  <include uri="webapp:editor/editor-commons.xml"/>
  <panel id="globalImport.person" lines="on">
    <hidden var="@ID"/>
    <hidden var="@label"/>
    <cell col="1" row="1" var="metadata">
      <panel>
        <cell col="1" row="10" anchor="NORTHWEST" ref="Lheading"/>
        <cell col="2" row="10" anchor="NORTHWEST" ref="Dheading" var="def.heading"/>
        <cell col="3" row="10" anchor="WEST" ref="Hheading"/>

        <cell col="1" row="20" anchor="NORTHWEST" ref="Lalternative"/>
        <cell col="2" row="20" anchor="NORTHWEST" ref="Dalternative" var="def.alternative"/>
        <cell col="3" row="20" anchor="WEST" ref="Halternative"/>

        <cell col="1" row="30" anchor="NORTHWEST" ref="Lpeerage"/>
        <cell col="2" row="30" anchor="NORTHWEST" ref="Dpeerage" var="def.peerage"/>
        <cell col="3" row="30" anchor="WEST" ref="Hpeerage"/>

        <cell col="1" row="40" anchor="NORTHWEST" ref="Lgender"/>
        <cell col="2" row="40" anchor="NORTHWEST" ref="Dgender" var="def.gender"/>
        <cell col="3" row="40" anchor="WEST" ref="Hgender"/>

        <cell col="1" row="42" anchor="NORTHWEST" ref="Lcontact"/>
        <cell col="2" row="42" anchor="NORTHWEST" ref="Dcontact" var="def.contact"/>
        <cell col="3" row="42" anchor="NORTHWEST" ref="Hcontact"/>

        <cell col="1" row="50" anchor="NORTHWEST" ref="Lrole"/>
        <cell col="2" row="50" anchor="NORTHWEST" ref="Drole" var="def.role"/>
        <cell col="3" row="50" anchor="WEST" ref="Hrole"/>

        <cell col="1" row="60" anchor="NORTHWEST" ref="LplaceOfActivity"/>
        <cell col="2" row="60" anchor="NORTHWEST" ref="DplaceOfActivity" var="def.placeOfActivity"/>
        <cell col="3" row="60" anchor="WEST" ref="HplaceOfActivity"/>

        <cell col="1" row="70" anchor="NORTHWEST" ref="LdateOfBirth"/>
        <cell col="2" row="70" anchor="NORTHWEST" ref="DdateOfBirth" var="def.dateOfBirth"/>
        <cell col="3" row="70" anchor="WEST" ref="HdateOfBirth"/>

        <cell col="1" row="80" anchor="NORTHWEST" ref="LplaceOfBirth"/>
        <cell col="2" row="80" anchor="NORTHWEST" ref="DplaceOfBirth" var="def.placeOfBirth"/>
        <cell col="3" row="80" anchor="WEST" ref="HplaceOfBirth"/>

        <cell col="1" row="90" anchor="NORTHWEST" ref="LdateOfDeath"/>
        <cell col="2" row="90" anchor="NORTHWEST" ref="DdateOfDeath" var="def.dateOfDeath"/>
        <cell col="3" row="90" anchor="WEST" ref="HdateOfDeath"/>

        <cell col="1" row="100" anchor="NORTHWEST" ref="LplaceOfDeath"/>
        <cell col="2" row="100" anchor="NORTHWEST" ref="DplaceOfDeath" var="def.placeOfDeath"/>
        <cell col="3" row="100" anchor="WEST" ref="HplaceOfDeath"/>

        <cell col="1" row="110" anchor="NORTHWEST" ref="Lnote"/>
        <cell col="2" row="110" anchor="NORTHWEST" ref="Dnote" var="def.note"/>
        <cell col="3" row="110" anchor="WEST" ref="Hnote"/>

        <cell col="1" colspan="2" row="120" anchor="EAST" ref="TwoButtons"/>
        <cell col="2" row="120"/>
      </panel>
    </cell>
  </panel>
  <panel id="personName">
    <cell col="1" row="1" anchor="NORTHWEST" var="lastName" i18n="editormask.person.lastName"/>
    <cell col="2" row="1" anchor="NORTHWEST" var="lastName">
      <textfield width="16" maxlength="128"/>
    </cell>
    <cell col="1" row="2" anchor="NORTHWEST" var="firstName" i18n="editormask.person.firstName"/>
    <cell col="2" row="2" anchor="NORTHWEST" var="firstName">
      <textfield width="16" maxlength="128"/>
    </cell>
  </panel>
<!-- *** Ansetzungsform: Name ** -->
  <panel id="Dheading">
    <hidden var="@class" default="MCRMetaXML"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="heading" ref="personName"/>
  </panel>
  <text id="Lheading" i18n="editormask.person.Lheading"/>
  <helpPopup id="Hheading" width="400" height="250" i18n="editormask.person.Hheading"/>
<!-- *** Namensformen ** -->
  <panel id="Dalternative">
    <hidden var="@class" default="MCRMetaXML"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="alternative[@type='complete']">
      <repeater min="0" max="10" ref="personName"/>
    </cell>
    <cell col="1" row="2" anchor="NORTHWEST" var="alternative[@type='single']">
      <repeater min="0" max="10">
        <panel>
          <cell var="name">
            <textfield width="35" maxlength="128"/>
          </cell>
        </panel>
      </repeater>
    </cell>
  </panel>
  <text id="Lalternative" i18n="editormask.person.Lalternative"/>
  <helpPopup id="Halternative" width="400" height="250" i18n="editormask.person.Halternative"/>
<!-- *** Geschlecht ** -->
  <panel id="Dgender">
    <hidden var="@class" default="MCRMetaClassification"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="gender">
      <panel>
        <hidden var="@classid" default="urmel_class_00000001"/>
        <hidden var="@inherited" default="0"/>
        <cell var="@categid">
          <list width="300" type="radio" default="unknown">
            <include uri="classification:editor:-1:children:urmel_class_00000001"/>
          </list>
        </cell>
      </panel>
    </cell>
  </panel>
  <text id="Lgender" i18n="editormask.person.Lgender"/>
  <helpPopup id="Hgender" width="400" height="250" i18n="editormask.person.Hgender"/>
<!-- *** Contact ** -->
  <panel id="Dcontact">
    <hidden var="@class" default="MCRMetaLangText"/>
    <hidden var="@parasearch" default="true"/>
    <hidden var="@textsearch" default="true"/>
    <hidden var="@heritable" default="false"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="contact" ordernr="1">
      <repeater min="0" max="9" anchor="NORTHWEST">
        <panel id="Dcontact_repeater">
          <cell row="1" anchor="NORTHWEST" ref="contact_type" var="@type"/>
          <cell row="3" anchor="NORTHWEST" ref="TFarea4096"/>
        </panel>
      </repeater>
    </cell>
  </panel>
  <list id="contact_type" type="dropdown">
	<include uri="classification:editor:-1:children:urmel_class_00000002" />
  </list>
  <text id="Lcontact" i18n="metaData.person.contact"/>
  <helpPopup id="Hcontact" width="400" height="250" i18n="contactData.accountData"/>
<!-- *** Adelstitel ** -->
  <panel id="Dpeerage">
    <hidden var="@class" default="MCRMetaLangText"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="peerage" ref="TFzeile128"/>
  </panel>
  <text id="Lpeerage" i18n="editormask.person.Lpeerage"/>
  <helpPopup id="Hpeerage" width="400" height="250" i18n="editormask.person.Hpeerage"/>
<!-- *** Ämter und Funktionen ** -->
  <panel id="Drole">
    <hidden var="@class" default="MCRMetaLangText"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="role">
      <repeater min="0" max="10" ref="repeater32"/>
    </cell>
  </panel>
  <text id="Lrole" i18n="editormask.person.Lrole"/>
  <helpPopup id="Hrole" width="400" height="250" i18n="editormask.person.Hrole"/>
<!-- *** Lebens- und Wirkungsorte ** -->
  <panel id="DplaceOfActivity">
    <hidden var="@class" default="MCRMetaLangText"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="placeOfActivity">
      <repeater min="0" max="10" ref="repeater32"/>
    </cell>
  </panel>
  <text id="LplaceOfActivity" i18n="editormask.person.LplaceOfActivity"/>
  <helpPopup id="HplaceOfActivity" width="400" height="250" i18n="editormask.person.HplaceOfActivity"/>
<!-- *** Geburtsdatum ** -->
  <panel id="DdateOfBirth">
    <hidden var="@class" default="MCRMetaISO8601Date"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="dateOfBirth" ref="TFzeile10"/>
  </panel>
  <text id="LdateOfBirth" i18n="editormask.person.LdateOfBirth"/>
  <helpPopup id="HdateOfBirth" width="400" height="250" i18n="editormask.person.HdateOfBirth"/>
<!-- *** Geburtsort ** -->
  <panel id="DplaceOfBirth">
    <hidden var="@class" default="MCRMetaLangText"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="placeOfBirth" ref="TFzeile128"/>
  </panel>
  <text id="LplaceOfBirth" i18n="editormask.person.LplaceOfBirth"/>
  <helpPopup id="HplaceOfBirth" width="400" height="250" i18n="editormask.person.HplaceOfBirth"/>
<!-- *** Sterbedatum ** -->
  <panel id="DdateOfDeath">
    <hidden var="@class" default="MCRMetaISO8601Date"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
    <cell col="1" row="1" anchor="NORTHWEST" var="dateOfDeath" ref="TFzeile10"/>
  </panel>
  <text id="LdateOfDeath" i18n="editormask.person.LdateOfDeath"/>
  <helpPopup id="HdateOfDeath" width="400" height="250" i18n="editormask.person.HdateOfDeath"/>
<!-- *** Sterbeort ** -->
  <panel id="DplaceOfDeath">
    <cell col="1" row="1" anchor="NORTHWEST" var="placeOfDeath" ref="TFzeile128"/>
    <hidden var="@class" default="MCRMetaLangText"/>
    <hidden var="@notinherit" default="true"/>
    <hidden var="@heritable" default="true"/>
  </panel>
  <text id="LplaceOfDeath" i18n="editormask.person.LplaceOfDeath"/>
  <helpPopup id="HplaceOfDeath" width="400" height="250" i18n="editormask.person.HplaceOfDeath"/>
</components>
