<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "web-app_2_3.dtd">
<web-app>
      <display-name>MyCoRe JPortal-Module</display-name>

      <servlet id="MCRJPortalCLIServlet">
            <servlet-name>MCRJPortalCLIServlet</servlet-name>
            <servlet-class>org.mycore.frontend.servlets.MCRJPortalCLIServlet</servlet-class>
      </servlet>
           
      <servlet-mapping>
            <servlet-name>MCRJPortalCLIServlet</servlet-name>
            <url-pattern>/cli</url-pattern>
      </servlet-mapping>
      

      <servlet id="MCRDublicateFinderServlet">
            <servlet-name>MCRDublicateFinderServlet</servlet-name>
            <servlet-class>org.mycore.frontend.servlets.MCRDublicateFinderServlet</servlet-class>
      </servlet>

      <servlet-mapping>
            <servlet-name>MCRDublicateFinderServlet</servlet-name>
            <url-pattern>/servlets/MCRDublicateFinderServlet/*</url-pattern>
      </servlet-mapping>


      <servlet id="MCRJPortalCreateContext">
            <servlet-name>MCRJPortalCreateJournalContextServlet</servlet-name>
            <servlet-class>org.mycore.frontend.servlets.MCRJPortalCreateJournalContextServlet</servlet-class>
      </servlet>

      <servlet-mapping>
            <servlet-name>MCRJPortalCreateJournalContextServlet</servlet-name>
            <url-pattern>/servlets/MCRJPortalCreateJournalContextServlet/*</url-pattern>
      </servlet-mapping>


</web-app>