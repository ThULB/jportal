      <!--get person / institution list ======================================================-->	
	<panel id="getPersonAndInst">
         <cell row="1" col="1" anchor="NORTHWEST" ref="personInst_label" />				   
         <cell row="1" col="2" width="100%">
		       <output var="@label" />
               <text   label=" (" />
			   <output var="@href" />
               <text   label=")" />				 
          </cell>
		 <cell row="2" col="1" >
				<subselect id="sub.select1" type="webpage"
					href="indexpage?searchclass=jpperson_sub&amp;mode=prefix&amp;search=xxxxxxxxxxxxxxxxxxxxx" i18n="editormask.common.sub.select1"/>			 
          </cell>			   
		  <cell row="2" col="2" >
				<subselect id="sub.select2" type="webpage"
					href="indexpage?searchclass=jpinst_sub&amp;mode=prefix&amp;search=xxxxxxxxxxxxxxxxxxxxx" i18n="editormask.common.sub.select2"/>		  
         </cell>			   			   
         <cell row="3" col="1" width="100%">
               <text   label="-" />
         </cell>			   	
	</panel>
      <!--get institution list ======================================================-->	
	<panel id="getInst">
         <cell row="1" col="1" anchor="NORTHWEST" ref="inst_label" />				   
         <cell row="1" col="2" width="100%">
		       <output var="@label" />
               <text   label=" (" />
			   <output var="@href" />
               <text   label=")" />				 
          </cell>
		  <cell row="2" col="2" >
				<subselect id="sub.select2" type="webpage"
					href="indexpage?searchclass=jpinst_sub&amp;mode=prefix&amp;search=xxxxxxxxxxxxxxxxxxxxx" sortnr="6" i18n="editormask.common.sub.select2"/>				 			  
         </cell>			   			   
         <cell row="3" col="1" width="100%">
               <text   label="-" />
         </cell>			   	
	</panel>

      <!-- participants ================================================================== -->
      <panel id="participants_input">
        <hidden var="metadata/participants/@class" default="MCRMetaLinkID" sortnr="3" />
        <hidden var="metadata/participants/@parasearch" default="true" sortnr="3" />
        <hidden var="metadata/participants/@textsearch" default="true" sortnr="3" />
	    <cell col="1" row="1" anchor="EAST" var="metadata/participants/participant" sortnr="3" >
         <repeater min="1" max="20">
           <panel lines="off">
			 <hidden var="@inherited" default="0" />
			 <hidden var="@type" default="locator" />
			 <hidden var="@label" />
			 <hidden var="@href" />			
             <cell col="1" row="1" anchor="NORTHWEST" ref="participants_input_repeater_type" var="@title" />			   
			 <cell row="2" col="1" anchor="NORTHWEST" ref="getPersonAndInst"/>
           </panel>
         </repeater>
       </cell>	  
	  </panel>	
