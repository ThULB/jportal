      <panel id="DTitle">
        <include ref="inherit.heritable" />
        <cell col="1" row="1" anchor="WEST" var="title" >
          <repeater min="1" max="5" pos="right">
            <panel>
              <hidden var="@inherited" default="0"/>
              <cell col="1" row="1" anchor="NORTHWEST">
                <textarea id="TFCoTitle" width="62" height="3" maxlength="1024">
                  <condition id="CoTitle" required="true">
                    <label xml:lang="de">Bitte tragen Sie den Titel ein!</label>
                    <label xml:lang="en">Please enter the title!</label>
