<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "web-app_2_3.dtd">
<web-app>
      <display-name>MyCoRe IView Module</display-name>

      <!-- **************************************************************** -->
      <!-- Servlets and mapping for the IView Module          -->
      <!-- **************************************************************** -->
 
      <servlet id="MCRIViewServlet">
            <servlet-name>MCRIViewServlet</servlet-name>
            <servlet-class>org.mycore.frontend.iview.MCRIViewServlet</servlet-class>
      </servlet>
           
      <servlet-mapping>
            <servlet-name>MCRIViewServlet</servlet-name>
            <url-pattern>/servlets/MCRIViewServlet/*</url-pattern>
      </servlet-mapping>
      
</web-app>
