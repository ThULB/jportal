<?xml version="1.0" encoding="UTF-8"?>
<journalListCfg>
	<journalListDef>
		<fileName>journalList.xml</fileName>
		<query>
			<sortBy>
				<field name="maintitles" order="ascending" />
			</sortBy>
			<conditions format="xml">
				<boolean operator="AND">
					<condition field="objectType" operator="=" value="jpjournal" />
					<condition field="deletedFlag" operator="=" value="false" />
					<boolean operator="NOT">
						<condition field="contentClassi1" operator="=" value="calendar" />
					</boolean>
				</boolean>
			</conditions>
		</query>
	</journalListDef>
	<journalListDef>
		<fileName>calendarList.xml</fileName>
		<query>
			<sortBy>
				<field name="maintitles" order="ascending" />
			</sortBy>
			<conditions format="xml">
				<boolean operator="AND">
					<condition field="objectType" operator="=" value="jpjournal" />
					<condition field="deletedFlag" operator="=" value="false" />
					<condition field="contentClassi1" operator="=" value="calendar" />
				</boolean>
			</conditions>
		</query>
	</journalListDef>
</journalListCfg>
