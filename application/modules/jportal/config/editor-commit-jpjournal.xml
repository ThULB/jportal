<?xml version="1.0" encoding="UTF-8"?>
<!-- ?xml version="1.0" encoding="ISO-8859-1"?-->
<!DOCTYPE editor>
<editor>
    <target type="servlet" name="MCRJPortalCheckCommitDataServlet" method="post" format="xml" />
    <source uri="{sourceUri}" />
    <components root="editor-jpjournal" var="/mycoreobject">
        <include uri="webapp:editor/imports-jpjournal.xml" />
        <panel id="editor-jpjournal" lines="on">
            <cell row="1" col="1" anchor="NORTHWEST" ref="globalImport.jpjournal" />
        </panel>
    </components>
    <cancel url="servlets/MCRJPortalEditorCancelServlet" />
</editor>