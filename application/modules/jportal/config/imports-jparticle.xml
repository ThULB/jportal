<?xml version="1.0" encoding="ISO-8859-1"?>
<imports>
    <include uri="webapp:editor/imports-jpcommon.xml" />
    <include uri="webapp:editor/imports-jplabelsHelptexts.xml" />
    <panel id="globalImport.jparticle" lines="on">
        <hidden var="ID" />
        <hidden var="label" />
        <hidden var="metadata/@lang" default="de" />
        <hidden var="service/servdates" descendants="true" />
        <hidden var="service/servacls" descendants="true" />
        <hidden var="service/servflags" descendants="true" />
        <!-- ########################## formal description ################################## -->
        <cell anchor="WEST" colspan="4" col="1" row="1" class="editorHeadlines">
            <text>
                <label i18n="metaData.headlines.formaldiscr" />
            </text>
        </cell>
        <!-- maintitle -->
        <cell class="editorMandatory" row="10" col="1" anchor="NORTHWEST" ref="maintitle_label" />
        <cell row="10" col="2" anchor="NORTHWEST" ref="maintitle_input" />
        <!-- subtitle -->
        <cell class="editorLabel" row="20" col="1" anchor="NORTHWEST" ref="subtitle_label" />
        <cell row="20" col="2" anchor="NORTHWEST" ref="subtitle_input" />
        <!-- author -->
        <cell class="editorLabel" row="30" col="1" anchor="NORTHWEST" ref="participants_label" />
        <cell row="30" col="2" anchor="NORTHWEST" ref="participants_input" />
        <!-- date -->
        <cell class="editorLabel" row="45" col="1" anchor="NORTHWEST" ref="date_label" />
        <cell row="45" col="2" anchor="NORTHWEST" ref="date_input" />
        <!-- reference -->
        <cell class="editorLabel" row="46" col="1" anchor="NORTHWEST" ref="pub_reference_label" />
        <cell row="46" col="2" anchor="NORTHWEST" ref="ref_input" />
        <!-- identi -->
        <cell class="editorLabel" row="47" col="1" anchor="NORTHWEST" ref="identi_label" />
        <cell row="47" col="2" anchor="NORTHWEST" ref="identi_input" />
        <!-- size -->
        <cell class="editorLabel" row="48" col="1" anchor="NORTHWEST" ref="size_label" />
        <cell row="48" col="2" anchor="NORTHWEST" ref="size_input" />
        <cell col="1" row="50">
            <br />
        </cell>
        <!-- ########################## content description ################################## -->
        <cell anchor="WEST" colspan="4" col="1" row="51" class="editorHeadlines">
            <text>
                <label css-style="editorHeadlines" i18n="metaData.headlines.contantdiscr" />
            </text>
        </cell>
        <!-- keyword -->
        <cell class="editorLabel" row="60" col="1" anchor="NORTHWEST" ref="keyword_label" />
        <cell row="60" col="2" anchor="NORTHWEST" ref="keyword_input" />
        <!-- abstract -->
        <cell class="editorLabel" row="70" col="1" anchor="NORTHWEST" ref="abstract_label" />
        <cell row="70" col="2" anchor="NORTHWEST" ref="abstract_input" />
        <!-- note -->
        <cell class="editorLabel" row="120" col="1" anchor="NORTHWEST" ref="note_label" />
        <cell row="120" col="2" anchor="NORTHWEST" ref="note_input" />
        <!-- dynamic includes, if used within a journal -->
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_rubricsID/hidden_rubricID:editor/editor-dynamicInludes.xml:include_rubric-editable_code_head" />
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_classispub/hidden_classipub:editor/editor-dynamicInludes.xml:include_classipub-editable_code_head" />
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_classispub2/hidden_classipub2:editor/editor-dynamicInludes.xml:include_classipub2-editable_code_head" />
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_classispub3/hidden_classipub3:editor/editor-dynamicInludes.xml:include_classipub3-editable_code_head" />
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_classispub4/hidden_classipub4:editor/editor-dynamicInludes.xml:include_classipub4-editable_code_head" />
        <include cacheable="false"
            uri="jportal_includeEditorCode:jparticle:hidden_pubTypesID/hidden_pubTypeID:editor/editor-dynamicInludes.xml:include_pubType-editable_code_head" />
        <!-- place holder -->
        <cell row="180" col="1" anchor="NORTHWEST">
            <space height="14" />
        </cell>
        <cell col="1" row="190">
            <br />
        </cell>
        <!-- ########################## misc ################################## -->
        <cell anchor="WEST" colspan="4" col="1" row="191" class="editorHeadlines">
            <text>
                <label css-style="editorHeadlines" i18n="metaData.headlines.misc" />
            </text>
        </cell>
        <!-- parent -->
        <cell class="editorMandatory" row="200" col="1" anchor="NORTHWEST" ref="parent_label" />
        <cell row="200" col="2" anchor="NORTHWEST" ref="parent_input_hided" />
        <!-- Hidden information to be used in heritated objects (volumes, articles) -->
        <cell row="250" col="2" anchor="NORTHWEST" ref="jpjournalID_input_hidden" />
        <!-- ================================================================================================= -->
        <!-- save / cancel button -->
        <cell row="300" col="1" anchor="EAST" />
        <cell row="300" col="2" anchor="EAST" ref="save" />
    </panel>
    <!-- panel definitions -->
    <!-- MainTitle ================================================================== -->
    <panel id="maintitle_input">
        <hidden var="metadata/maintitles/@class" default="MCRMetaLangText" />
        <hidden var="metadata/maintitles/@heritable" default="false" />
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFarea4096" var="metadata/maintitles/maintitle" />
    </panel>
    <!-- end: MainTitle -->
    <!-- SubTitle ================================================================== -->
    <panel id="subtitle_input">
        <hidden var="metadata/subtitles/@class" default="MCRMetaLangText" />
        <hidden var="metadata/subtitles/@heritable" default="false" />
        <cell col="1" row="1" anchor="NORTHWEST" var="metadata/subtitles/subtitle" ordernr="1">
            <repeater min="0" max="50" anchor="NORTHWEST" ref="subtitle_input_repeater" />
        </cell>
    </panel>
    <panel id="subtitle_input_repeater">
        <cell col="1" row="1" anchor="NORTHWEST" ref="subtitle_type" var="@type" ordernr="1" />
        <cell col="1" row="3" anchor="NORTHWEST" ref="TFarea4096" var="" ordernr="1" />
    </panel>
    <!-- date ================================================================== -->
    <panel id="date_input">
        <hidden var="metadata/dates/@class" default="MCRMetaISO8601Date" />
        <hidden var="metadata/dates/@heritable" default="false" />
        <cell col="1" row="1" anchor="NORTHWEST" var="metadata/dates/date" ordernr="1">
            <repeater min="0" max="2" anchor="NORTHWEST" ref="date_input_repeater" />
        </cell>
    </panel>
    <panel id="date_input_repeater">
        <cell col="1" row="1" anchor="NORTHWEST" ref="date_type" var="@type" ordernr="1" />
        <cell col="1" row="3" anchor="NORTHWEST" ref="TFzeile10" var="" ordernr="1" />
        <cell col="1" row="2" anchor="NORTHWEST" ref="date_howToUse" />
    </panel>
    <!-- end: date -->
    <!-- keyword ================================================================== -->
    <panel id="keyword_input">
        <hidden var="metadata/keywords/@class" default="MCRMetaLangText" />
        <hidden var="metadata/keywords/@heritable" default="true" />
        <cell col="2" row="1" anchor="NORTHWEST" var="metadata/keywords/keyword" ordernr="1">
            <repeater min="0" max="60" anchor="NORTHWEST" ref="keyword_input_repeater" />
        </cell>
    </panel>
    <panel id="keyword_input_repeater">
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile1024" var="" ordernr="1" />
    </panel>
    <!-- end: keyword -->
    <!-- abstract ================================================================== -->
    <panel id="abstract_input">
        <hidden var="metadata/abstracts/@class" default="MCRMetaLangText" />
        <hidden var="metadata/abstracts/@heritable" default="false" />
        <cell col="1" row="1" anchor="NORTHWEST" var="metadata/abstracts/abstract" ordernr="1">
            <repeater min="0" max="3" anchor="NORTHWEST" ref="abstract_input_repeater" />
        </cell>
    </panel>
    <panel id="abstract_input_repeater">
        <!--		<cell col="1" row="1" anchor="NORTHWEST" ref="abstract_type" var="@type" ordernr="1"/>-->
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFarea4096" var="" ordernr="1" />
    </panel>
    <!-- identi ================================================================== -->
    <panel id="identi_input">
        <hidden var="metadata/identis/@class" default="MCRMetaLangText" />
        <hidden var="metadata/identis/@heritable" default="false" />
        <cell col="1" row="1" anchor="NORTHWEST" var="metadata/identis/identi" ordernr="1">
            <repeater min="0" max="10" anchor="NORTHWEST" ref="identi_input_repeater" />
        </cell>
    </panel>
    <panel id="identi_input_repeater">
        <cell col="1" row="1" anchor="NORTHWEST" ref="identi_type" var="@type" ordernr="1" />
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile1024" var="" ordernr="1" />
    </panel>
    <!-- note ================================================================== -->
    <panel id="note_input">
        <hidden var="metadata/notes/@class" default="MCRMetaLangText" />
        <hidden var="metadata/notes/@heritable" default="false" />
        <cell col="1" row="1" anchor="NORTHWEST" var="metadata/notes/note">
            <repeater min="0" max="10" anchor="NORTHWEST" ref="note_input_repeater" />
        </cell>
    </panel>
    <panel id="note_input_repeater">
        <cell col="1" row="1" anchor="NORTHWEST" ref="note_type" var="@type" ordernr="1" />
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFarea4096" var="" />
    </panel>
    <!-- ================================================================== -->
    <!-- dynamic includes, if used within a journal -->
    <include cacheable="false"
        uri="jportal_includeEditorCode:jparticle:hidden_rubricsID/hidden_rubricID:editor/editor-dynamicInludes.xml:include_rubric-editable_code" />
    <include cacheable="false"
        uri="jportal_includeEditorCode:jparticle:hidden_classispub/hidden_classipub:editor/editor-dynamicInludes.xml:include_classipub-editable_code" />
    <include cacheable="false"
        uri="jportal_includeEditorCode:jparticle:hidden_classispub2/hidden_classipub2:editor/editor-dynamicInludes.xml:include_classipub2-editable_code" />
    <include cacheable="false"
        uri="jportal_includeEditorCode:jparticle:hidden_classispub3/hidden_classipub3:editor/editor-dynamicInludes.xml:include_classipub3-editable_code" />
    <include cacheable="false"
        uri="jportal_includeEditorCode:jparticle:hidden_classispub4/hidden_classipub4:editor/editor-dynamicInludes.xml:include_classipub4-editable_code" />
    <include cacheable="false"
        uri="jportal_includeEditorCode:jparticle:hidden_pubTypesID/hidden_pubTypeID:editor/editor-dynamicInludes.xml:include_pubType-editable_code" />
    <!-- size ================================================================== -->
    <panel id="size_input">
        <hidden var="metadata/sizes/@class" default="MCRMetaLangText" />
        <hidden var="metadata/sizes/@heritable" default="false" />
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile128" var="metadata/sizes/size" ordernr="1" />
    </panel>
    <!-- ref ================================================================== -->
    <panel id="ref_input">
        <hidden var="metadata/refs/@class" default="MCRMetaLangText" />
        <hidden var="metadata/refs/@heritable" default="false" />
        <cell col="1" row="1" anchor="NORTHWEST" var="metadata/refs/ref" ordernr="1">
            <repeater min="0" max="10" anchor="NORTHWEST" ref="ref_input_repeater" />
        </cell>
    </panel>
    <panel id="ref_input_repeater">
        <cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile128" var="" ordernr="1" />
    </panel>
    <!--date types =======================================================================-->
    <list id="date_type" type="dropdown">
        <include uri="classification:editor:-1:children:jportal_class_00000008" />
    </list>
</imports>