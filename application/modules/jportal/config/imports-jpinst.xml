<?xml version="1.0" encoding="ISO-8859-1"?>
<imports>
	<include uri="webapp:editor/imports-jpcommon.xml"/>
	<include uri="webapp:editor/imports-jplabelsHelptexts.xml"/> s
	<panel id="globalImport.jpinst" lines="on">
		<hidden var="ID"/>
		<hidden var="label"/>
		<hidden var="metadata/@lang" default="de"/>
		<hidden var="service/servdates" descendants="true"/>
		<hidden var="service/servacls" descendants="true"/>
		<hidden var="service/servflags" descendants="true"/>
		<!--name-->
		<cell class="editorMandatory" col="1" row="10" anchor="NORTHWEST" ref="LINameP_bold"/>
		<cell col="2" row="10" anchor="NORTHWEST" ref="DIName"/>
		<cell col="3" row="10" anchor="NORTHWEST"><space width="20" height="0"/></cell>
		<!-- address -->
		<cell col="1" row="20" anchor="NORTHWEST" ref="LAddress" class="editorLabel"/>
		<cell col="2" row="20" anchor="NORTHWEST" ref="DAddress"/>
		<cell col="3" row="20" anchor="NORTHWEST">
			<space width="20" height="0"/>
		</cell>
		<!-- phone -->
		<cell col="1" row="30" anchor="NORTHWEST" ref="LPhone" class="editorLabel"/>
		<cell col="2" row="30" anchor="NORTHWEST" ref="DPhone"/>
		<cell col="3" row="30" anchor="NORTHWEST">
			<space width="20" height="0"/>
		</cell>
		<!--URL -->
		<cell col="1" row="40" anchor="NORTHWEST" ref="LURL" class="editorLabel"/>
		<cell col="2" row="40" anchor="NORTHWEST" ref="DURL"/>
		<cell col="3" row="40" anchor="NORTHWEST">
			<space width="20" height="0"/>
		</cell>
		<!--email-->
		<cell col="1" row="50" anchor="NORTHWEST" ref="LEmail" class="editorLabel"/>
		<cell col="2" row="50" anchor="NORTHWEST" ref="DEmail"/>
		<cell col="3" row="50" anchor="NORTHWEST">
			<space width="20" height="0"/>
		</cell>
		<!-- note -->
		<cell col="1" row="60" anchor="NORTHWEST" ref="LNote" class="editorLabel"/>
		<cell col="2" row="60" anchor="NORTHWEST" ref="DNote"/>
		<cell col="3" row="60" anchor="NORTHWEST">
			<space width="20" height="0"/>
		</cell>
		<!-- save / cancel button -->
		<cell row="190" col="1" anchor="EAST"/>
		<cell row="190" col="2" anchor="EAST" ref="save"/>
	</panel>
	<!-- ========================================================================================================================= -->
	<panel id="DIName">
		<hidden var="metadata/names/@class" default="MCRMetaInstitutionName"/>
		<hidden var="metadata/names/@notinherit" default="false"/>
		<hidden var="metadata/names/@heritable" default="true"/>
		<hidden var="metadata/names/name/@inherited" default="0"/>
		<cell col="2" row="1" anchor="NORTHWEST" ref="TFzeile128" var="metadata/names/name/fullname" ordernr="1"/>
		<cell col="3" row="1" anchor="NORTHWEST" ref="AINameName"/>
		<cell col="2" row="2" anchor="NORTHWEST" ref="TFzeile32" var="metadata/names/name/nickname" ordernr="1"/>
		<cell col="3" row="2" anchor="NORTHWEST" ref="AINameNick"/>
		<cell col="2" row="3" anchor="NORTHWEST" ref="TFzeile32" var="metadata/names/name/property" ordernr="1"/>
		<cell col="3" row="3" anchor="NORTHWEST" ref="AINameProp"/>
	</panel>
	<!-- ========================================================================================================================= -->	
	<panel id="DAddress">
		<hidden var="metadata/addresses/@class" default="MCRMetaAddress"/>
<!--		<hidden var="metadata/addresses/@notinherit" default="false"/>
		<hidden var="metadata/addresses/@heritable" default="true"/>
		<hidden var="metadata/addresses/address/@inherited" default="0"/> -->
		<cell col="2" row="1" anchor="NORTHWEST" var="metadata/addresses/address/country" ref="TFzeile128"/>
		<cell col="3" row="1" anchor="NORTHWEST" ref="AAddressCountry"/>
		<cell col="2" row="3" anchor="NORTHWEST" ref="TFzeile10" var="metadata/addresses/address/zipcode" ordernr="2"/>
		<cell col="3" row="3" anchor="NORTHWEST" ref="AAddressZIP"/>
		<cell col="2" row="4" anchor="NORTHWEST" ref="TFzeile128" var="metadata/addresses/address/city" ordernr="2"/>
		<cell col="3" row="4" anchor="NORTHWEST" ref="AAddressCity"/>
		<cell col="2" row="5" anchor="NORTHWEST" ref="TFzeile128" var="metadata/addresses/address/street" ordernr="2"/>
		<cell col="3" row="5" anchor="NORTHWEST" ref="AAddressStreet"/>
		<cell col="2" row="6" anchor="NORTHWEST" ref="TFzeile10" var="metadata/addresses/address/number" ordernr="2"/>
		<cell col="3" row="6" anchor="NORTHWEST" ref="AAddressNumber"/> 
	</panel>
	<!-- ========================================================================================================================= -->	
	<panel id="DPhone">
		<hidden var="metadata/phones/@class" default="MCRMetaLangText"/>
		<hidden var="metadata/phones/@notinherit" default="false"/>
		<hidden var="metadata/phones/@heritable" default="true"/>
		<cell col="1" row="1" anchor="NORTHWEST" var="metadata/phones/phone">
			<repeater min="1" max="5" ref="DPhoneRef"/>
		</cell>
	</panel>
	<!-- ========================================================================================================================= -->
	<panel id="DPhoneRef">
		<hidden var="@inherited" default="0"/>
		<hidden var="@lang" default="de"/>
		<cell col="1" row="1" anchor="NORTHWEST" ref="Sphone" var="@type" sortnr="3"/>
		<cell col="1" row="2" anchor="NORTHWEST" ref="TFzeile128" sortnr="3"/>
	</panel>
	<!-- ========================================================================================================================= -->	
	<panel id="DURL">
		<hidden var="metadata/urls/@class" default="MCRMetaLink"/>
		<hidden var="metadata/urls/@notinherit" default="false"/>
		<hidden var="metadata/urls/@heritable" default="true"/>
		<hidden var="metadata/urls/url/@inherited" default="0"/>
		<hidden var="metadata/urls/url/@type" default="locator"/>
		<cell col="1" row="1" anchor="NORTHWEST">
			<space width="80" height="0"/>
		</cell>
		<cell col="2" row="1" anchor="NORTHWEST" ref="TFzeile128"
			var="metadata/urls/url/@title" ordernr="4"/>
		<cell col="3" row="1" anchor="NORTHWEST" ref="AURLTitle"/>
		<cell col="2" row="2" anchor="NORTHWEST" ref="TFzeile128"
			var="metadata/urls/url/@href" ordernr="4"/>
		<cell col="3" row="2" anchor="NORTHWEST" ref="AURLReference"/>
	</panel>
	<!-- ========================================================================================================================= -->	
	<panel id="DEmail">
		<hidden var="metadata/emails/@class" default="MCRMetaLangText"/>
		<hidden var="metadata/emails/@notinherit" default="false"/>
		<hidden var="metadata/emails/@heritable" default="true"/>
		<hidden var="metadata/emails/email/@inherited" default="0"/>
		<cell col="2" row="1" anchor="NORTHWEST" ref="TFzeile128"
			var="metadata/emails/email" ordernr="5"/>
	</panel>
	<!-- ========================================================================================================================= -->	
	<panel id="DNote">
		<hidden var="metadata/notes/@class" default="MCRMetaLangText"/>
		<hidden var="metadata/notes/@notinherit" default="false"/>
		<hidden var="metadata/notes/@heritable" default="true"/>
		<hidden var="metadata/notes/note/@inherited" default="0"/>
		<cell col="2" row="1" anchor="NORTHWEST" ref="TFarea4096"
			var="metadata/notes/note" ordernr="6" default=""/>
	</panel>
 </imports>