<?xml version="1.0" encoding="ISO-8859-1"?>
<imports>
      <include uri="webapp:editor/imports-jpcommon.xml" />
      <include uri="webapp:editor/imports-jplabelsHelptexts.xml" />
      <panel id="globalImport.jpvolume" lines="on">
            <hidden var="ID" />
            <hidden var="label" />
            <hidden var="metadata/@lang" default="de" />
			<hidden var="service/servdates" descendants="true"/>
			<hidden var="service/servacls" descendants="true"/>
			<hidden var="service/servflags" descendants="true"/>		  
            <cell row="1" col="1" anchor="NORTHWEST" ref="parent_label" />
            <cell row="1" col="2" anchor="NORTHWEST" ref="parent_input_hided" />
            <!-- maintitle -->
            <cell row="10" col="1" anchor="NORTHWEST" ref="maintitle_label" />
            <cell row="10" col="2" anchor="NORTHWEST" ref="maintitle_input" />
            <cell row="10" col="3" anchor="WEST" ref="maintitle_help" />
            <!-- subtitle -->
            <cell row="20" col="1" anchor="NORTHWEST" ref="subtitle_label" />
            <cell row="20" col="2" anchor="NORTHWEST" ref="subtitle_input" />
            <cell row="20" col="3" anchor="NORTHWEST" ref="subtitle_help" />
            <!-- partic -->
            <cell row="30" col="1" anchor="NORTHWEST" ref="participants_label" />
            <cell row="30" col="2" anchor="NORTHWEST" ref="participants_input" />
            <!-- date -->
            <cell row="75" col="1" anchor="NORTHWEST" ref="date_label" />
            <cell row="75" col="2" anchor="NORTHWEST" ref="date_input" />
            <!-- note -->
            <cell row="160" col="1" anchor="NORTHWEST" ref="note_label" />
            <cell row="160" col="2" anchor="NORTHWEST" ref="note_input" />
            <cell row="160" col="3" anchor="WEST" ref="note_help" />
		    <!-- ================================================================================================= -->
		    <!-- Hidden information to be used in heritated objects (volumes, articles) -->                            		  
            <cell row="170" col="2" anchor="NORTHWEST" ref="jpjournalID_input_hidden" />		  
            <!-- position -->
            <cell row="180" col="1" anchor="NORTHWEST" ref="position_label" />
            <cell row="180" col="2" anchor="NORTHWEST" ref="position_input" />
            <!-- .. -->
<!--            <cell row="210" col="2" anchor="NORTHWEST" ref="rubricID_input_hidden" />
            <cell row="220" col="2" anchor="NORTHWEST" ref="pubTypeID_input_hidden" />
			<cell row="230" col="2" anchor="NORTHWEST" ref="classipub_input_hidden"/>
			<cell row="240" col="2" anchor="NORTHWEST" ref="classipub2_input_hidden"/>
			<cell row="250" col="2" anchor="NORTHWEST" ref="classipub3_input_hidden"/>				  
            <cell row="260" col="2" anchor="NORTHWEST" ref="websitecontext_input_hidden" />		  		  		  		  
            <cell row="270" col="2" anchor="NORTHWEST" ref="genhiddenfield1_input_hidden" />		  		  		  		  
            <cell row="280" col="2" anchor="NORTHWEST" ref="genhiddenfield2_input_hidden" />		  		  		  		  		  		  		  
            <cell row="290" col="2" anchor="NORTHWEST" ref="genhiddenfield3_input_hidden" />	-->			  
		    <!-- ================================================================================================= -->		  
            <cell row="190" col="2" anchor="EAST" ref="save" />
      </panel>
      <!-- panel definitions -->
      <!-- MainTitle ================================================================== -->
      <panel id="maintitle_input">
            <hidden var="metadata/maintitles/@class" default="MCRMetaLangText" />
            <hidden var="metadata/maintitles/@parasearch" default="true" />
            <hidden var="metadata/maintitles/@textsearch" default="true" />
<!--            <hidden var="metadata/maintitles/@notinherit" default="false" />-->
            <hidden var="metadata/maintitles/@heritable" default="true" />
            <hidden var="metadata/maintitles/maintitle/@inherited" default="0" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="Slang" var="metadata/maintitles/maintitle/@lang" 
                  default="de" />
            <cell col="1" row="2" anchor="WEST" ref="TFarea4096" var="metadata/maintitles/maintitle" />
      </panel>
      <!-- end: MainTitle -->
      <!-- SubTitle ================================================================== -->
      <panel id="subtitle_input">
            <hidden var="metadata/subtitles/@class" default="MCRMetaLangText" />
            <hidden var="metadata/subtitles/@parasearch" default="true" />
            <hidden var="metadata/subtitles/@textsearch" default="true" />
            <hidden var="metadata/subtitles/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/subtitles/subtitle" >
                  <repeater min="0" max="20" anchor="NORTHWEST" ref="subtitle_input_repeater" />
            </cell>
      </panel>
      <panel id="subtitle_input_repeater">
            <cell col="1" row="1" anchor="NORTHWEST" ref="subtitle_type" var="@type" />		  
            <cell col="1" row="2" anchor="NORTHWEST" ref="Slang" var="@lang" default="de" />		  
            <cell col="1" row="3" anchor="NORTHWEST" ref="TFarea4096"  />
      </panel>
      <!-- end: SubTitle -->
      <!-- date ================================================================== -->
      <panel id="date_input">
            <hidden var="metadata/dates/@class" default="MCRMetaISO8601Date" />
            <hidden var="metadata/dates/@parasearch" default="true" />
            <hidden var="metadata/dates/@textsearch" default="true" />
            <hidden var="metadata/dates/@heritable" default="true" />
            <cell col="1" row="1" anchor="NORTHWEST" var="metadata/dates/date" ordernr="1" >
                  <repeater min="0" max="2" anchor="NORTHWEST" ref="date_input_repeater" />
            </cell>
      </panel>
      <panel id="date_input_repeater">
            <cell col="1" row="1" anchor="NORTHWEST" ref="date_type" var="@type" ordernr="1" />		  
            <cell col="1" row="3" anchor="NORTHWEST" ref="TFzeile10" var="" ordernr="1" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="date_howToUse" />
      </panel>    
    
      <!-- end: date -->
      <!-- note ================================================================== -->
      <panel id="note_input">
            <hidden var="metadata/notes/@class" default="MCRMetaLangText" />
            <hidden var="metadata/notes/@parasearch" default="true" />
            <hidden var="metadata/notes/@textsearch" default="true" />
            <hidden var="metadata/notes/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="Slang" var="metadata/notes/note/@lang" ordernr="1" default="de" />
            <cell col="1" row="2" anchor="NORTHWEST" ref="TFarea4096" var="metadata/notes/note" ordernr="1" />
      </panel>
      <!-- end: note -->
      <!-- hidden_position ================================================================== -->
      <panel id="position_input">
            <hidden var="metadata/hidden_positions/@class" default="MCRMetaLangText" />
            <hidden var="metadata/hidden_positions/@parasearch" default="true" />
            <hidden var="metadata/hidden_positions/@textsearch" default="true" />
            <hidden var="metadata/hidden_positions/@heritable" default="false" />
            <cell col="1" row="1" anchor="NORTHWEST" ref="TFzeile10" var="metadata/hidden_positions/hidden_position"  />
      </panel>
      <!-- end: hidden_position -->	
      <!--date =======================================================================-->
      <list id="date_type" type="dropdown">
		<include uri="classification:editor:-1:children:jportal_class_00000009"/>	    
      </list>	        
    
</imports>