<?xml version="1.0" encoding="ISO_8859-1"?>
<!DOCTYPE MyCoReWebPage>

<MyCoReWebPage>
  <section title="Neueingabe eines Objektes" xml:lang="de" />
  <section title="Create a new object" xml:lang="en" />
  <section xml:lang="all">
    <p class="editorform">
      <editor id="author-file">
        <source uri="jportal_getParentID:{parentID}" />
        <source uri="{sourceUri}" />
        <cancel url="servlets/MCRJPortalEditorCancelServlet" />

        <validationMessage>
          <label xml:lang="de">Eingabefehler: Bitte korrigieren Sie die markierten Felder.</label>
          <label xml:lang="en">Type error: Please correct the marked fields.</label>
        </validationMessage>

        <target type="servlet" name="changedByXsl" method="post" format="xml" />
        <components root="editor-file" var="/mycoreobject"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

          <include uri="webapp:editor/editor-file.xml" />
          <panel id="editor-file">
            <cell row="1" col="1" anchor="NORTHWEST" ref="edit-file" />
          </panel>

        </components>
      </editor>
    </p>
  </section>
</MyCoReWebPage>