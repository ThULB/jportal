<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "web-app_2_3.dtd">
<web-app>
      <display-name>MyCoRe user administration</display-name>

      <!-- **************************************************************** -->
      <!-- Servlets and mapping for the user administration module          -->
      <!-- **************************************************************** -->
 
      <servlet id="MCRUserAdmin">
            <servlet-name>MCRUserAdminServlet</servlet-name>
            <servlet-class>org.mycore.frontend.servlets.MCRUserAdminServlet</servlet-class>
      </servlet>
      <servlet id="MCRUserEditor">
            <servlet-name>MCRUserEditorServlet</servlet-name>
            <servlet-class>org.mycore.frontend.servlets.MCRUserEditorServlet</servlet-class>
      </servlet>
           
      <servlet-mapping>
            <servlet-name>MCRUserAdminServlet</servlet-name>
            <url-pattern>/servlets/MCRUserAdminServlet</url-pattern>
      </servlet-mapping>
      <servlet-mapping>
            <servlet-name>MCRUserEditorServlet</servlet-name>
            <url-pattern>/servlets/MCRUserEditorServlet</url-pattern>
      </servlet-mapping>
      
</web-app>
