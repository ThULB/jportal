<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.mycore.backend.hibernate.tables">
   <class name="MCRRESUMPTIONTOKEN" table="MCRRESUMPTIONTOKEN">
      <id column="RESUMPTIONTOKENID" name="resumptionTokenID" type="string">
         <generator class="assigned" />
      </id>
      <property name="prefix" type="string" column="PREFIX" length="64" not-null="false" unique="false" />
      <property name="completeSize" type="long" column="COMPLETESIZE" not-null="false" unique="false" />
      <property name="created" type="timestamp" column="CREATED" not-null="false" unique="false" />
      <property name="instance" type="string" column="INSTANCE" length="128" not-null="false" unique="false" />
      <property name="hitBlob" type="blob" column="HITBLOB" length="7000000" not-null="false" unique="false" />
   </class>
</hibernate-mapping>