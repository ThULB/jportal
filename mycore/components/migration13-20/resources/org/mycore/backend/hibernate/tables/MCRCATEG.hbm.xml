<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.mycore.backend.hibernate.tables">
  <class name="MCRCATEG" table="MCRCATEG">
    <composite-id class="MCRCATEGPK" name="key">
      <key-property name="id" type="string">
        <column name="id" index="MCRCATEG_INDEX" length="128" />
      </key-property>
      <key-property name="clid" type="string">
        <column name="clid" index="MCRCATEG_INDEX" length="64" />
      </key-property>
    </composite-id>
    <property name="pid" type="string" column="PID" length="128" not-null="false" unique="false" />
    <property name="url" type="string" column="URL" length="254" not-null="true" unique="false" />
  </class>
</hibernate-mapping>

